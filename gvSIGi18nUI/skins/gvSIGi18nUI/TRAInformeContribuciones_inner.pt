      lang="en">
<body>



<div metal:define-macro="tInformeContribuciones_inner" >

                    
    <tal:block tal:replace="nothing" >
    <h2>
        HERE GOES THE CONTRIBUTIONS REPORT
        
        <textarea
            readonly="readonly"
            rows="24"
            columns="72"
            name="theInformeContribuciones_data_Display_Field" 
            id="theInformeContribuciones_data_Display_Field" 
            style="font-size: 8pt;"
            tal:content="python: here.getInformeContribuciones()" />
    </h2>
    <br/>
    <br/>
    <br/>
    </tal:block>



    <tal:block tal:define="
        global pStartTime       pStartTime | python: here.MDDModelDDvlPlone_tool.fMillisecondsNow();
        pExecutionRecord        python: here.TRAgvSIGi18n_tool.fStartExecution( here, 'template', 'TRACatalogoActividad_inner.pt', None, True, { 'log_what': 'details', 'log_when': True, });
        pPermissionsCache       pPermissionsCache | python: {};
        pRolesCache             pRolesCache | python: {};
        pInformeContribuciones  pInformeContribuciones | nothing;
        pDisplayCountryFlags    python: here.TRAgvSIGi18n_tool.fDisplayCountryFlags( here);
        pHereURL                python: '%s/Tabular/' % here.TRAgvSIGi18n_tool.fAbsoluteURL( here,);
        pURLAllIdiomas          python: '%s#cidAllIdiomas' % pHereURL;
        pURLAllUsers            python: '%s#cidAllUsers' % pHereURL;
        aTranslationsCache       python: {};
        dummy       python:           here.TRAgvSIGi18n_tool.fTranslateI18NManyIntoDict( here, [
            [ 'gvSIGi18n', [    
                [ 'gvSIGi18n_fechaInformeContribuciones_label',                                      'Contributions Report Date-'  ],
                [ 'gvSIGi18n_cadenas_msgid',                                                         'Strings-' ,            ],
                [ 'gvSIGi18n_total_msgid',                                                           'Total-' ,              ],
                [ 'gvSIGi18n_ContributionsReport_Period_Today',                                      'Today-' ,              ],
                [ 'gvSIGi18n_ContributionsReport_Period_Yesterday',                                  'Yesterday-' ,              ],
                [ 'gvSIGi18n_ContributionsReport_Period_Last7Days',                                  'Last 7 Days-' ,              ],
                [ 'gvSIGi18n_ContributionsReport_Period_Last30Days',                                 'Last 30 Days-' ,              ],
                [ 'gvSIGi18n_ContributionsReport_Period_Last365Days',                                'Last 365 Days-' ,              ],
                [ 'gvSIGi18n_ContributionsReport_Period_Before365Days',                              'Before 365 Days-' ,              ],
                [ 'gvSIGi18n_bandera_label',                                                         'Flag-' ,               ],
                [ 'gvSIGi18n_TRAIdioma_attr_codigoIdiomaEnGvSIG_label_abbreviated',                  'Code-' ,               ],
                [ 'gvSIGi18n_TRAIdioma_attr_codigoInternacionalDeIdioma_label_abbreviated',          'Intl. Code-' ,         ],
                [ 'gvSIGi18n_TRAIdioma_attr_nombreIdioma_label_abbreviated',                         'Name-' ,               ],
                [ 'gvSIGi18n_TRAIdioma_attr_nombreNativoDeIdioma_label_abbreviated',                 'Native name-' ,        ],
                [ 'gvSIGi18n_LanguageLocked_label',                                                  'Locked-' ,               ],
                [ 'gvSIGi18n_ContributionsTotals_label',                                             'Contributions Totals-' ,               ],
                [ 'gvSIGi18n_ContributionsByLanguages_label',                                        'Contributions by Languages-' ,               ],
                [ 'gvSIGi18n_ContributionsByLanguagesDetails_label',                                 'Details of Contributions by Languages-' ,               ],
                [ 'gvSIGi18n_ContributionsReport_Details',                                           'Details-' ,               ],
                [ 'gvSIGi18n_ContributionsTotals_WithDetails_label',                                 'Contributions Totals with Details-',               ],
                
                [ 'gvSIGi18n_ContributionsByLanguages_WithDetails_label',                            'Contributions by Languages with Details-' ,               ],
                
                [ 'gvSIGi18n_ContributionsRaw_label',                                                'Raw Contributions report (expression in Python language)-' ,               ],
                
                [ 'gvSIGi18n_ContributionsUsuarios_TodosIdiomas_label',                              'Contributions from Users for all Languages-' ,               ],
                [ 'gvSIGi18n_ContributionsUsuarios_TodosIdiomas_WithDetails_label',                  'Contributions from Users for all Languages (with Details) -' ,               ],
                
                [ 'gvSIGi18n_ContributionsAdmins_TodosIdiomas_label',                                'Contributions from Administrators for all Languages-' ,               ],
                [ 'gvSIGi18n_ContributionsAdmins_TodosIdiomas_WithDetails_label',                    'Contributions from Administrators for all Languages (with Details) -' ,               ],
                
                [ 'gvSIGi18n_Contributions_Idioma_label',                                            'Contributions for Language-' ,               ],
                [ 'gvSIGi18n_Contributions_Idioma_WithDetails_label',                                'Contributions for Language (with Details) -' ,               ],
                [ 'gvSIGi18n_ContributionsUsuarios_Idioma_label',                                    'Contributions from Users for Language-' ,               ],
                [ 'gvSIGi18n_ContributionsUsuarios_Idioma_WithDetails_label',                        'Contributions from Users for Language (with Details) -' ,               ],
                [ 'gvSIGi18n_ContributionsAdmins_Idioma_label',                                      'Contributions from Administrators for Language-' ,               ],
                [ 'gvSIGi18n_ContributionsAdmins_Idioma_WithDetails_label',                          'Contributions from Administrators for Language  (with Details)-' ,               ],
                
                [ 'gvSIGi18n_ContributionsUsuarios_NoReportForIdioma_label',                         'No Report Available about Contributions for Language-' ,               ],
                
                
                [ 'gvSIGi18n_Languages',                                                             'Languages-' ,               ],
                
                
                [ 'gvSIGi18n_MemberId',                                                              'Member id-' ,               ],
                [ 'gvSIGi18n_UserName',                                                              'User name-' ,               ],
                [ 'gvSIGi18n_UserPhoto',                                                             'Photo-' ,               ],
                
                [ 'gvSIGi18n_ScrollUpToAllLanguages',                                                'Scroll Up To All Languages-' ,               ],
                [ 'gvSIGi18n_ScrollUpToAllUsers',                                                    'Scroll Up To All Users-' ,               ],
                                                    
                [ 'gvSIGi18n_Details_help',                                                          'Details legend: Cr=Created, Tr=Translated, Re=Reviewed, De=Definitive(locked)-' ,               ],
                
              
            ]]
        ], aTranslationsCache);" >
        
        <tal:block tal:condition="python: (not pInformeContribuciones) or not pInformeContribuciones.get( 'success', False)" >
            <br/>
            <h3 tal:content="python: here.TRAgvSIGi18n_tool.fTranslateI18N( here, 'gvSIGi18n', 'gvSIGi18n_noHayInfomeContribuciones_msgid',   'No Contributions Report available-' )" />
            <br/>
        </tal:block>

        <tal:block tal:condition="python: pInformeContribuciones and pInformeContribuciones.get( 'success', False)" >
            <h2>
               <span tal:content="python: here.TRAgvSIGi18n_tool.fTranslateI18N( here, 'gvSIGi18n', 'gvSIGi18n_ContributionsReport_label',   'Report for Contributions to the Translations Catalog-' )" />
            </h2>         
            
               
            <br/>
            <br/>
            
            
            <p>
                <font size="2">
                    <em tal:content="python: here.TRAgvSIGi18n_tool.fTranslateI18N( here, 'gvSIGi18n', 'gvSIGi18n_fechaInformeContribuciones_label', 'Contributions Report date-' )" />
                    &nbsp;
                    <strong tal:content="python: here.TRAgvSIGi18n_tool.fAsUnicode( here, pInformeContribuciones.get( 'report_date', ''))" />            
                </font>
            </p>
            
            <br/>
            <br/>
            
            <tal:block tal:replace="nothing" >
                <dl id="id_TRAInformeContribuciones_Raw_collapsible" class="collapsible inline collapsedInlineCollapsible">
                    <dt class="collapsibleHeader">
                        <strong tal:content="aTranslationsCache/gvSIGi18n_ContributionsRaw_label" />
                    </dt>
                    <dd class="collapsibleContent">    
                        <br/>
                        <h2>
                            <strong tal:content="aTranslationsCache/gvSIGi18n_ContributionsRaw_label" />
                        </h2>
                        <br/>
                        <tal:block tal:replace="structure python: here.MDDModelDDvlPlone_tool.fPrettyPrintHTML( here, [ pInformeContribuciones,],)" />
                        <br/>
                        <br/>
                    </dd>
                </dl>
                <br/>
                <br/>
            </tal:block>
            
            
            
            
            
            <tal:block tal:define="
                pPeriodKeys              python: pInformeContribuciones.get( 'period_keys', []);
                pModesKeys               python: pInformeContribuciones.get( 'modes_keys', []);
                pLanguageCodes           python: pInformeContribuciones.get( 'sorted_language_codes', []);
                
                pTotalContributions      python: pInformeContribuciones.get( 'total_contributions', 0);
                pTotalContributionsByMode python: pInformeContribuciones.get( 'total_contributions_by_mode', {});
                pPeriodsTotals           python: pInformeContribuciones.get( 'periods_totals', {});
                pContributionsByLanguage python: pInformeContribuciones.get( 'contributions_by_language', {});
                
                pContributionsByUser     python: pInformeContribuciones.get( 'contributions_by_user', {});
                
                
                pUsersMemberInfo         python: pInformeContribuciones.get( 'users_member_info', {});
                
                pRankUserIdsTotal        python: pInformeContribuciones.get( 'rank_user_ids_total', []);
                pRankAdminUserIdsTotal   python: pInformeContribuciones.get( 'rank_admin_user_ids_total', []);
                
                pClasesFilas             python: ['even', 'odd',];
                global pIndex            python: 0;
                
                pHayCodigosIdiomasInternacionalesDistintosCodigoEnGvSIG python: len( [ unInformeIdioma for unInformeIdioma in  pContributionsByLanguage.values() if not ( unInformeIdioma.get( 'codigoIdiomaEnGvSIG', '') == unInformeIdioma.get( 'codigoInternacionalDeIdioma', unInformeIdioma.get( 'codigoIdiomaEnGvSIG', '')))]) > 0">
                
          

                
                <dl id="id_TRAInformeContribuciones_Totals_collapsible" class="collapsible inline expandedInlineCollapsible">
                    <dt class="collapsibleHeader">
                        <strong tal:content="aTranslationsCache/gvSIGi18n_ContributionsTotals_label" />
                    </dt>
                    <dd class="collapsibleContent">    
                        <br/>
                        
                
                        <dl id="id_TRAInformeContribuciones_Totals_WithDetails_collapsible" class="collapsible inline collapsedInlineCollapsible">
                            <dt class="collapsibleHeader">
                                <span style="font-weight: normal;" tal:content="aTranslationsCache/gvSIGi18n_ContributionsTotals_WithDetails_label" />
                            </dt>
                            <dd class="collapsibleContent">    
                                <br/>
                                <h2>
                                    <strong tal:content="aTranslationsCache/gvSIGi18n_ContributionsTotals_WithDetails_label" />
                                </h2>
                                <p class="formHelp" tal:content="aTranslationsCache/gvSIGi18n_Details_help" />
                                
                                <table id="id_TRAInformeContribuciones_Totals_WithDetails_Table" class="listing" summary="#"  tal:attributes="summary string: Contributions Report Totals with Details">
                                    <col class="nosort"  />
                                    <col class="nosort"  />
                                    
                                    <tal:block tal:repeat="unPeriodo pPeriodKeys">
                                        <col class="nosort" />
                                        <col class="nosort" />
                                    </tal:block>
                                    
                                    <thead>
                                        <tr >
                                            <th class="nosort" align="center"  tal:content="aTranslationsCache/gvSIGi18n_total_msgid" />
                                            <th class="nosort" align="center"  tal:content="python: '%s %s' % ( aTranslationsCache.get( 'gvSIGi18n_total_msgid'), aTranslationsCache['gvSIGi18n_ContributionsReport_Details'], )" />
                                            
                                            <tal:block tal:repeat="unPeriodo pPeriodKeys">
                                                <th class="nosort" align="center" tal:content="python: aTranslationsCache.get( 'gvSIGi18n_ContributionsReport_Period_%s' % unPeriodo, 'gvSIGi18n_ContributionsReport_Period_%s' % unPeriodo)" />
                                                <th class="nosort" align="center" tal:content="python: '%s %s' % ( aTranslationsCache.get( 'gvSIGi18n_ContributionsReport_Period_%s' % unPeriodo, 'gvSIGi18n_ContributionsReport_Period_%s' % unPeriodo), aTranslationsCache['gvSIGi18n_ContributionsReport_Details'],)" />
                                            </tal:block>
                                        </tr>
                                    </thead>
                                    
                                    <tbody>
                                        <tr class="odd" >
                                            <td valign="top" tal:content="python: '%d' % pTotalContributions" />
                                            
                                            <td valign="top" align="right"  >
                                                <tal:block tal:repeat="pModoContribucion pModesKeys" >
                                                    <span tal:condition="python: pTotalContributionsByMode.get( pModoContribucion,0)"
                                                        tal:content="python: '%d_%s' % ( pTotalContributionsByMode.get( pModoContribucion,0), pModoContribucion[:2],)" />
                                                    <br tal:condition="python: pTotalContributionsByMode.get( pModoContribucion,0)"/>
                                                </tal:block>
                                            </td>
                                             
                                            <tal:block tal:repeat="unPeriodo pPeriodKeys" >
                                                <tal:block tal:define="pInformePeriodo python: pPeriodsTotals.get( unPeriodo, {})" >
                                                
                                                    <td style="border-left: 1px solid" valign="top" align="right" 
                                                        tal:content="python: pInformePeriodo.get( 'total_contributions', 0)  or ' '" />
                                                    
                                                    <td style="border-right: 1px solid" valign="top" align="right"  >
                                                        <tal:block tal:condition="python: pInformePeriodo.get( 'total_contributions', 0)"
                                                            tal:repeat="pModoContribucion pModesKeys" >
                                                            <span tal:condition="python: pInformePeriodo.get( 'contributions_by_mode', {}).get( pModoContribucion,0)"
                                                                tal:content="python: '%d_%s' % ( pInformePeriodo.get( 'contributions_by_mode', {}).get( pModoContribucion,0), pModoContribucion[:2],)" />
                                                            <br tal:condition="python: pInformePeriodo.get( 'contributions_by_mode', {}).get( pModoContribucion,0)"/>
                                                        </tal:block>
                                                
                                                    </td>
                                                </tal:block>
                                            </tal:block>
                                         </tr>
                                    </tbody>
                                </table>                    
                            </dd>
                        </dl>
                        <br/>
                        <br/>
                        
                        
                        
                        <h2>
                            <strong tal:content="aTranslationsCache/gvSIGi18n_ContributionsTotals_label" />
                        </h2>
                        
                        <table id="id_TRAInformeContribuciones_Totals_Table" class="listing" summary="#"  tal:attributes="summary string: Contributions Report Totals">
                            <col class="nosort"  />
                            
                            <tal:block tal:repeat="unPeriodo pPeriodKeys">
                                <col class="nosort" />
                            </tal:block>
                            
                            <thead>
                                <tr>
                                    <th class="nosort" align="right"  tal:content="aTranslationsCache/gvSIGi18n_total_msgid" />
                                    <tal:block tal:repeat="unPeriodo pPeriodKeys">
                                        <th class="nosort" align="center" tal:content="python: aTranslationsCache.get( 'gvSIGi18n_ContributionsReport_Period_%s' % unPeriodo, 'gvSIGi18n_ContributionsReport_Period_%s' % unPeriodo)" />
                                    </tal:block>
                                </tr>
                            </thead>
                            
                            <tbody>
                                <tr class="odd" >
                                    <td valign="top" tal:content="python: '%d' % pTotalContributions" />
                                    
                                    <tal:block tal:repeat="unPeriodo pPeriodKeys" >
                                        <tal:block tal:define="pInformePeriodo python: pPeriodsTotals.get( unPeriodo, {})" >
                                        
                                            <td valign="top" align="right" 
                                                tal:content="python: pInformePeriodo.get( 'total_contributions', 0) or ' '" />
                                        </tal:block>
                                    </tal:block>
                                 </tr>
                            </tbody>
                        </table>   
                        

                    </dd>
                </dl>
                   
                <br/>
                <br/>

                
                
                
                
                
          
                <dl id="cidAllIdiomas" class="collapsible inline expandedInlineCollapsible">
                    <dt class="collapsibleHeader">
                        <strong tal:content="aTranslationsCache/gvSIGi18n_ContributionsByLanguages_label" />
                    </dt>
                    <dd class="collapsibleContent">    
                        <br/>
                        
                        <dl id="id_TRAInformeContribuciones_PorIdiomas_WithDetails_collapsible" class="collapsible inline collapsedInlineCollapsible">
                            <dt class="collapsibleHeader">
                                <span style="font-weight: normal;"  tal:content="aTranslationsCache/gvSIGi18n_ContributionsByLanguages_WithDetails_label" />
                            </dt>
                            <dd class="collapsibleContent">    
                                <br/>
                                <h2>
                                    <strong tal:content="aTranslationsCache/gvSIGi18n_ContributionsByLanguages_WithDetails_label" />
                                </h2>
                                <p class="formHelp" tal:content="aTranslationsCache/gvSIGi18n_Details_help" />
                                
                                <table id="id_TRAInformeContribuciones_PorIdiomas_With_Details_Table" class="listing" summary="#"  tal:attributes="summary string: Contributions Report by Languages with Details">
                                    <col width="40"  class="sortable" />
                                    <col  class="sortable" tal:condition="pHayCodigosIdiomasInternacionalesDistintosCodigoEnGvSIG" width="40" />
                                    <col width="70"  class="sortable" />
                                    <col  width="20" tal:condition="pDisplayCountryFlags" />
            
                                    <col class="sortable"  />
                                    <col class="sortable"  />
                                    
                                    <tal:block tal:repeat="unPeriodo pPeriodKeys">
                                        <col class="sortable" />
                                        <col class="nosort" />
                                    </tal:block>
                                    
                                    <thead>
                                        <tr>
                                            <th align="center" tal:content="aTranslationsCache/gvSIGi18n_TRAIdioma_attr_codigoIdiomaEnGvSIG_label_abbreviated" />
                                            <th tal:condition="pHayCodigosIdiomasInternacionalesDistintosCodigoEnGvSIG"
                                                align="center" tal:content="aTranslationsCache/gvSIGi18n_TRAIdioma_attr_codigoInternacionalDeIdioma_label_abbreviated" />
                                            <th align="center" tal:content="aTranslationsCache/gvSIGi18n_TRAIdioma_attr_nombreIdioma_label_abbreviated" />
                                            <th align="center" tal:content="aTranslationsCache/gvSIGi18n_TRAIdioma_attr_nombreNativoDeIdioma_label_abbreviated" />
                                            <th align="center" tal:condition="pDisplayCountryFlags"
                                                class="nosort" tal:content="aTranslationsCache/gvSIGi18n_bandera_label" />
                                            <th align="center" tal:content="aTranslationsCache/gvSIGi18n_total_msgid" />
                                            <th align="center" tal:content="python: '%s %s' % ( aTranslationsCache[ 'gvSIGi18n_total_msgid'], aTranslationsCache['gvSIGi18n_ContributionsReport_Details'],)" />
                                            
                                            <tal:block tal:repeat="unPeriodo pPeriodKeys">
                                                <th align="center" tal:content="python: aTranslationsCache.get( 'gvSIGi18n_ContributionsReport_Period_%s' % unPeriodo, 'gvSIGi18n_ContributionsReport_Period_%s' % unPeriodo)" />
                                                <th align="center" tal:content="python: '%s %s' % ( aTranslationsCache.get( 'gvSIGi18n_ContributionsReport_Period_%s' % unPeriodo, 'gvSIGi18n_ContributionsReport_Period_%s' % unPeriodo), aTranslationsCache['gvSIGi18n_ContributionsReport_Details'],)" />
                                            </tal:block>
                                        </tr>
                                    </thead>
                                    
                                    <tbody>
                                        <tal:block tal:repeat="unCodigoIdioma python: pLanguageCodes" >
                                            <tal:block tal:define="unInformeIdioma python: pContributionsByLanguage.get( unCodigoIdioma, { })" 
                                                tal:condition="unInformeIdioma" >
            
                                                <tr class="#" tal:attributes="class python: pClasesFilas[pIndex % 2]"
                                                    tal:define="global pIndex python: pIndex + 1;
                                                        pCode           python: here.TRAgvSIGi18n_tool.fAsUnicode( here, unInformeIdioma.get( 'codigo_idioma_en_gvsig', ''));
                                                        pIntlCode       python: here.TRAgvSIGi18n_tool.fAsUnicode( here, unInformeIdioma.get( 'codigo_internacional_idioma', ''));
                                                        pLanguageName   python: here.TRAgvSIGi18n_tool.fAsUnicode( here, unInformeIdioma.get( 'nombre_idioma', ''));
                                                        pNativeName     python: here.TRAgvSIGi18n_tool.fAsUnicode( here, unInformeIdioma.get( 'nombre_nativo_idioma', ''));
                                                        pFlag           python: unInformeIdioma.get( 'flag', '') or 'flag-ninguna.gif';
                                                        pFlagURL        python: unInformeIdioma.get( 'flag_url', '');
                                                        pSizesIdioma        python: here.TRASizesIdioma( pCode);
                                                        pURLIdioma python: '%s#cidIdioma_%s' % ( pHereURL, unInformeIdioma[ 'codigo_idioma_en_gvsig'], )" >
                                             
                                                    <td style="border-top: 1px solid" align="left" valign="baseline" >
                                                        <a href="#" title="#"  tal:attributes="href python:  pURLIdioma; title pLanguageName">
                                                            <font size="1" color="Black">
                                                                <strong tal:content="pCode" />
                                                            </font>
                                                        </a>
                                                    </td>
                                                    <td style="border-top: 1px solid" tal:condition="pHayCodigosIdiomasInternacionalesDistintosCodigoEnGvSIG" 
                                                        align="left" valign="baseline" >
                                                        <a tal:condition="python: not ( pIntlCode == pCode)"
                                                            href="#" title="#"  tal:attributes="href python:  pURLIdioma; title pLanguageName">
                                                            <font size="1" color="Black">
                                                                <strong tal:content="pIntlCode" />
                                                            </font>
                                                        </a>
                                                    </td>
                                                    <td style="border-top: 1px solid" align="left" valign="baseline" >
                                                        <a href="#" title="#"  tal:attributes="href python:  pURLIdioma; title pLanguageName">
                                                            <font size="1" color="Black">
                                                                <strong tal:content="pLanguageName" />
                                                            </font>
                                                        </a>
                                                    </td>
                                                    <td style="border-top: 1px solid" align="left" valign="baseline" >
                                                        <a href="#" title="#"  tal:attributes="href python:  pURLIdioma; title pLanguageName">
                                                            <font  color="Black"  size="#" tal:attributes="size python: pSizesIdioma.get( 'display_font_size', '1')">
                                                                <strong tal:content="pNativeName" />
                                                            </font>
                                                        </a>
                                                    </td>
                                                     <td style="border-top: 1px solid" align="left" valign="baseline" tal:condition="pDisplayCountryFlags" >
                                                        <a href="#" title="#"  tal:attributes="href python:  pURLIdioma; title pLanguageName">
                                                            <img width="14" height="11" alt="#" src="#"
                                                                tal:attributes="
                                                                    src python: pFlagURL;
                                                                    alt pLanguageName;
                                                                    title pLanguageName" />
                                                        </a>
                                                    </td>
                                    
                                                    <td style="border-top: 1px solid; border-left: 1px solid;" align="right" valign="top" 
                                                        tal:content="python: '%d' % unInformeIdioma.get( 'total_contributions', 0)" />
                                                    
                                                    <td style="border-top: 1px solid; border-right: 1px solid;" align="right" valign="top" >
                                                        <tal:block tal:condition="python: unInformeIdioma.get( 'total_contributions', 0)" 
