<body>                     

                             
    <div metal:define-macro="tTRAVerificationReport_i18n" >
        <tal:block tal:define="pVerifyOrInitResult pVerificationResult | nothing" tal:condition="pVerifyOrInitResult" >
            <metal:block metal:use-macro="here/TRAVerificationReport_i18n_view/macros/tTRAVerificationOrInitializationReport_i18n" />   
        </tal:block>
    </div>   
      
    
    
    <div metal:define-macro="tTRAInicializationReport_i18n" >
        <tal:block tal:define="pVerifyOrInitResult pInicializationResult | nothing"  tal:condition="pVerifyOrInitResult" >
            <metal:block metal:use-macro="here/TRAVerificationReport_i18n_view/macros/tTRAVerificationOrInitializationReport_i18n" />   
        </tal:block>
    </div>   
      
    
    
       
    
    
    
    
                             
    <div metal:define-macro="tTRAVerificationOrInitializationReport_i18n" >
        
        <tal:block tal:define="
            pVerifyOrInitResult      pVerifyOrInitResult | nothing;
            pOverallSuccess          python: pVerifyOrInitResult and pVerifyOrInitResult.get( 'success', False);
            pIsInitialization        python: pVerifyOrInitResult.get( 'process_type', '') == 'Inicializar';
            pVerifyOrInitAction      python: ( pVerifyOrInitResult and pVerifyOrInitResult.get( 'translated_process_type', False)) or 'Verify or Initialize'" >
       
            <h2>
                <tal:block tal:condition="not: pVerifyOrInitResult" >
                    <font color="red">
                        <strong  tal:content="python: here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_Error_NoResult_msgid',   'No result obtained -' )" />
                    </font>
                </tal:block>
                
                <tal:block tal:condition="pVerifyOrInitResult" >
                    <font tal:condition="pOverallSuccess"  color="green">
                       <strong tal:content="python: u'%s %s' % ( here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_Overall_msgid',   'Overall-' ), here.fTranslateI18N(  'ModelDDvlPlone', 'ModelDDvlPlone_Success_msgid',   'Success-' ),)" />
                    </font>
                    <font tal:condition="not: pOverallSuccess"  color="red">
                       <strong tal:content="python: u'%s %s' % ( here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_Overall_msgid',   'Overall-' ), here.fTranslateI18N(  'ModelDDvlPlone', 'ModelDDvlPlone_Failure_msgid',   'Failure-' ), )" />
                   </font>
                </tal:block>    
            </h2>
            <br/>
            <br/>

            <tal:block tal:condition="pVerifyOrInitResult" >

                    
                <tal:block tal:define="pCondition python: pVerifyOrInitResult.get( 'condition', '')" 
                    tal:condition="pCondition">
                    <p>
                        <strong tal:content="python: here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_VerifyOrInitialize_Condition_Label',   'Condition-' )" />
                        &ensp;
                        <font color="red" tal:content="python: pCondition" />
                    </p>
                    <br/>                
                </tal:block>
                
                
                <tal:block tal:define="pException python: pVerifyOrInitResult.get( 'exception', '')" 
                    tal:condition="pException">
                    <p>
                        <strong tal:content="python: here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_VerifyOrInitialize_Exception_Label',   'Exception-' )" />
                    </p>
                    <textarea readonly style="font-size: 8pt"  cols="80"  rows="#"
                        tal:define="pExceptionLines python: pException.splitlines()" 
                        tal:attributes="rows python: max( 2, min( len( pExceptionLines), 20))">
<tal:block tal:repeat="pExceptionLine pExceptionLines" tal:content="pExceptionLine"/>
</textarea>
                    <br/>
                </tal:block>
                
                

                    
                <tal:block tal:define="
                    pVerifyOrInitResult python: pVerifyOrInitResult.get( 'ModelDDvlPlone', None);
                    pVerifyOrInitName   python: here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_VerifyOrInitialize_Target_ModelDDvlPlone',   'ModelDDvlPlone framework',)" >
                    <metal:block metal:use-macro="here/MDDVerificationReport_i18n_view/macros/tMDDVerificationOrInitializationReport_i18n" />
                    <br/>
                </tal:block>
                                    
                    
                
                <tal:block tal:define="
                    pVerifyOrInitResult python: pVerifyOrInitResult.get( 'gvSIGi18nUI', None);
                    pVerifyOrInitName   python: here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_VerifyOrInitialize_Target_gvSIGi18nUI',   'gvSIGi18n User Interface',)" >
                    <metal:block metal:use-macro="here/MDDVerificationReport_i18n_view/macros/tMDDVerificationOrInitializationReport_i18n" />
                    <br/>
                </tal:block>     
                  
                
                    
                <tal:block tal:define="
                    pVerifyOrInitResult python: pVerifyOrInitResult.get( 'gvSIGi18nTool', None);
                    pVerifyOrInitName   python: here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_VerifyOrInitialize_Target_gvSIGi18nTool',   'gvSIGi18n boundary Tool',)" >
                    <metal:block metal:use-macro="here/MDDVerificationReport_i18n_view/macros/tMDDVerificationOrInitializationReport_i18n" />
                    <br/>
                </tal:block>     
                  
                    
                
                <tal:block tal:define="
                    pVerifyOrInitResult python: pVerifyOrInitResult.get( 'catalog', None);
                    pVerifyOrInitName   python: here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_VerifyOrInitialize_Target_Catalog',   'gvSIGi18n translations catalog root',)" >
                    <metal:block metal:use-macro="here/TRAVerificationReport_i18n_view/macros/tTRAVerificationOrInitializationReport_TranslationsCatalog_i18n" />
                    <br/>
                </tal:block>     
                  
                     
            </tal:block>
        </tal:block>
        <br/>
                      
    </div>

    
    
    
                             
    <div metal:define-macro="tTRAVerificationOrInitializationReport_TranslationsCatalog_i18n" >
        
        <tal:block tal:define="
            pVerifyOrInitResult      pVerifyOrInitResult | nothing;
            pOverallSuccess          python: pVerifyOrInitResult and pVerifyOrInitResult.get( 'success', False);
            pVerifyOrInitActionTitle python: u'%s %s %s' % ( pVerifyOrInitAction,  pVerifyOrInitName, here.TRAgvSIGi18n_tool.fCatalogoTitle( here,),)" >
       
            <h1>
                <tal:block tal:condition="not: pVerifyOrInitResult" >
                    <span tal:content="pVerifyOrInitActionTitle" />
                    :
                    <font color="red">
                        <strong  tal:content="python: here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_Error_NoResult_msgid',   'No result obtained -' )" />
                    </font>
                </tal:block>
                <tal:block tal:condition="pVerifyOrInitResult" >
                    <span tal:content="pVerifyOrInitActionTitle" />
                    :
                    <font tal:condition="pOverallSuccess"  color="green">
                       <strong tal:content="python: u'%s %s' % ( here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_Overall_msgid',   'Overall-' ), here.fTranslateI18N(  'ModelDDvlPlone', 'ModelDDvlPlone_Success_msgid',   'Success-' ),)" />
                    </font>
                    <font tal:condition="not: pOverallSuccess"  color="red">
                       <strong tal:content="python: u'%s %s' % ( here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_Overall_msgid',   'Overall-' ), here.fTranslateI18N(  'ModelDDvlPlone', 'ModelDDvlPlone_Failure_msgid',   'Failure-'  ), )" />
                   </font>
                </tal:block>    
            </h1>
            
            <br/>

            <tal:block tal:condition="pVerifyOrInitResult" >

                <tal:block tal:define="pOverallSuccess python: pVerifyOrInitResult.get( 'success', False)">
                
                    <table class="listing" tal:define="pClasesFilas python: [ 'even', 'odd',]; global pRowIndex python: 0" >
                        <thead>
                            <tr>
                                <th colspan="2" class="nosort"/>
                            </tr>
                        </thead>
                        <tbody>
                            <tr  class="#" tal:define="global pRowIndex python: pRowIndex + 1"  tal:attributes="class python: pClasesFilas[ pRowIndex % 2]" >
                                <td class="nosort">
                                    <strong tal:content="python:  here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_VerifyOrInitialize_Element_MetaType_Label',   'Element Type-' )" />
                                </td>
                                <td tal:content="python: pVerifyOrInitResult.get( 'element_meta_type', '')" />
                            </tr>
                            <tr  class="#" tal:define="global pRowIndex python: pRowIndex + 1"  tal:attributes="class python: pClasesFilas[ pRowIndex % 2]" >
                                <td>
                                    <strong tal:content="python: here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_VerifyOrInitialize_Element_Title_Label',   'Element Title-' )" />
                                </td>
                                <td tal:content="python: pVerifyOrInitResult.get( 'element_title', '')" />
                            </tr>
                            <tr  class="#" tal:define="global pRowIndex python: pRowIndex + 1"  tal:attributes="class python: pClasesFilas[ pRowIndex % 2]" >
                                <td>
                                    <strong tal:content="python: here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_VerifyOrInitialize_Element_Path_Label',   'Element Path-' )" />
                                </td>
                                <td tal:content="python: pVerifyOrInitResult.get( 'element_path', '')" />
                            </tr>
                        </tbody>
                    </table>
                    <br/>
                    <br/>
                        
                    

                    <tal:block tal:define="pMustRunReCatalogElements python: pVerifyOrInitResult.get( 'must_run_recatalog_elements', False)" >
                        <p>
                            <strong tal:content="python: here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_VerifyOrInitialize_MustRunRecatalogElements_Label',   'Must Run Recatalog Elements-' )" />
                            &ensp;
                            <font tal:condition="pMustRunReCatalogElements" color="red" size="3" tal:content="python: pMustRunReCatalogElements" />
                            <font tal:condition="not: pMustRunReCatalogElements" tal:content="python: pMustRunReCatalogElements" />
                        </p>
                    </tal:block>
                    
                    <tal:block tal:define="pCondition python: pVerifyOrInitResult.get( 'condition', '')" 
                        tal:condition="pCondition">
                        <p>
                            <strong tal:content="python: here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_VerifyOrInitialize_Condition_Label',   'Condition-' )" />
                            &ensp;
                            <font color="red" tal:content="python: pCondition" />
                        </p>
                        <br/>
                    </tal:block>
                    
                    <tal:block tal:define="pException python: pVerifyOrInitResult.get( 'exception', '')" 
                        tal:condition="pException">
                        <p>
                            <strong tal:content="python: here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_VerifyOrInitialize_Exception_Label',   'Exception-' )" />
                        </p>
                        <textarea readonly style="font-size: 8pt"  cols="80"  rows="#"
                            tal:define="pExceptionLines python: pException.splitlines()" 
                            tal:attributes="rows python: max( 2, min( len( pExceptionLines), 20))">
<tal:block tal:repeat="pExceptionLine pExceptionLines" tal:content="pExceptionLine"/>
</textarea>
                        <br/>
                    </tal:block>
                    
                    

                        
                        
                    <tal:block tal:define="pVerifyOrInitResult_Collections python: pVerifyOrInitResult.get( 'colecciones', None)" >
                        <metal:block metal:use-macro="here/TRAVerificationReport_i18n_view/macros/tTRAVerificationOrInitializationReport_Collections_i18n" />
                    </tal:block>     

                    <tal:block tal:define="pVerifyOrInitResult_Singletons python: pVerifyOrInitResult.get( 'early_singletons', None)" >
                        <metal:block metal:use-macro="here/TRAVerificationReport_i18n_view/macros/tTRAVerificationOrInitializationReport_Singletons_i18n" />
                    </tal:block>     

                    <tal:block tal:define="pVerifyOrInitResult_Singletons python: pVerifyOrInitResult.get( 'singletons', None)" >
                        <metal:block metal:use-macro="here/TRAVerificationReport_i18n_view/macros/tTRAVerificationOrInitializationReport_Singletons_i18n" />
                    </tal:block>     

                    <tal:block tal:define="pVerifyOrInitResult_Catalogs python: pVerifyOrInitResult.get( 'catalogs_cadenas', None)" >
                        <metal:block metal:use-macro="here/TRAVerificationReport_i18n_view/macros/tTRAVerificationOrInitializationReport_CatalogsCadenas_i18n" />
                    </tal:block>     

                    <tal:block tal:define="pVerifyOrInitResults_CatalogsTodosIdiomas python: pVerifyOrInitResult.get( 'catalogs_idiomas', None)" >
                        <metal:block metal:use-macro="here/TRAVerificationReport_i18n_view/macros/tTRAVerificationOrInitializationReport_CatalogsTodosIdiomas_i18n" />
                    </tal:block> 
                    

                    <tal:block tal:define="pVerifyOrInitResults_UserGroupsCatalogo python: pVerifyOrInitResult.get( 'user_groups_catalogo', None)" >
                        <metal:block metal:use-macro="here/TRAVerificationReport_i18n_view/macros/tTRAVerificationOrInitializationReport_UserGroupsCatalogo_i18n" />
                    </tal:block> 
                      
                </tal:block>
            </tal:block>
        </tal:block>
        <br/>
                      
    </div>

    
    
           
    <div metal:define-macro="tTRAVerificationOrInitializationReport_Collections_i18n" >
        
        <tal:block tal:define="
            pVerifyOrInitResult_Collections         pVerifyOrInitResult_Collections | nothing;
            pOverallSuccess                         python: pVerifyOrInitResult_Collections and pVerifyOrInitResult_Collections.get( 'success', False);
            pVerifyOrInitActionTitle                python: u'%s %s %s' % ( pVerifyOrInitAction, pVerifyOrInitName, here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_VerifyOrInitializeSectionTitle_Collections_msgid', 'Collections',),)" >
       
            <h3>
                <span tal:content="pVerifyOrInitActionTitle" />
                &nbsp;
                <span tal:condition="not: pVerifyOrInitResult_Collections" >
                    <font  color="red">
                        <strong  tal:content="python: u'%s %s' % ( pVerifyOrInitActionTitle, here.fTranslateI18N(  'ModelDDvlPlone', 'ModelDDvlPlone_Error_NoResult_msgid',   'No result obtained -' ),)" />
                    </font>
                </span>
                <tal:block tal:condition="pVerifyOrInitResult_Collections" >
                    <font tal:condition="pOverallSuccess"  color="green">
                       <strong tal:content="python: here.fTranslateI18N(  'ModelDDvlPlone', 'ModelDDvlPlone_Success_msgid',   'Success-' )" />
                    </font>
                    <font tal:condition="not: pOverallSuccess"  color="red">
                       <strong tal:content="python:  here.fTranslateI18N(  'ModelDDvlPlone', 'ModelDDvlPlone_Failure_msgid',   'Failure-' )" />
                   </font>
                </tal:block>    
            </h3>

            <tal:block tal:condition="pVerifyOrInitResult_Collections" >
            
                <table class="listing" tal:define="pClasesFilas python: [ 'even', 'odd',]; global pRowIndex python: 0" >
                    <thead>
                        <tr>
                            <th colspan="2" class="nosort"/>
                        </tr>
                    </thead>
                    <tbody>
                        <tr  class="#" tal:define="global pRowIndex python: pRowIndex + 1"  tal:attributes="class python: pClasesFilas[ pRowIndex % 2]" >
                            <td class="nosort">
                                <strong tal:content="python:  here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_VerifyOrInitialize_Container_MetaType_Label',   'Container Type-' )" />
                            </td>
                            <td tal:content="python: pVerifyOrInitResult_Collections.get( 'container_type', '')" />
                        </tr>
                        <tr  class="#" tal:define="global pRowIndex python: pRowIndex + 1"  tal:attributes="class python: pClasesFilas[ pRowIndex % 2]" >
                            <td>
                                <strong tal:content="python: here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_VerifyOrInitialize_Container_Title_Label',   'Container Title-' )" />
                            </td>
                            <td tal:content="python: pVerifyOrInitResult_Collections.get( 'container_title', '')" />
                        </tr>
                        <tr  class="#" tal:define="global pRowIndex python: pRowIndex + 1"  tal:attributes="class python: pClasesFilas[ pRowIndex % 2]" >
                            <td>
                                <strong tal:content="python: here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_VerifyOrInitialize_Container_Path_Label',   'Container Path-' )" />
                            </td>
                            <td tal:content="python: pVerifyOrInitResult_Collections.get( 'container_path', '').replace( '/', '/ ')" />
                        </tr>
                    </tbody>
                </table>
                <br/>
                <br/>
                                
            
                <tal:block tal:define="pCollections python:  pVerifyOrInitResult_Collections.get( 'collections', [])" >
                    <tal:block tal:condition="not: pCollections">
                        <p>
                            <strong tal:content="python: here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_Verification_NoCollections_msgid',   'No Collections-' )" />
                        </p>
                    </tal:block>
                    <tal:block tal:condition="pCollections">
                        <p>
                            <strong tal:content="python: '%d %s' % ( len( pCollections), here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_Verification_CollectionsNumber_msgid',   'Collections-' ))" />
                        </p>
                        <table id="id_VerificationOrInitializationReport_Collections_Table" class="listing" summary="#"  tal:attributes="summary string:Verification or Initialization Report Collections">
                            <thead>
                                <tr>
                                    <th class="sortable" tal:content="python:here.fTranslateI18N(  'ModelDDvlPlone', 'ModelDDvlPlone_Success_msgid',   'Success-' )" />
                                    <th class="sortable" tal:content="python:here.fTranslateI18N(  'ModelDDvlPlone', 'ModelDDvlPlone_Status_msgid',   'Status-' )" />
                                    <th tal:condition="pIsInitialization" class="sortable" tal:content="python:here.fTranslateI18N(  'ModelDDvlPlone', 'ModelDDvlPlone_Committed_msgid',   'Committed-' )" />
                                    <th class="sortable" tal:content="python:here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_Verification_Collection_Type_msgid',   'Type-' )" />
                                    <th class="sortable" tal:content="python:here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_Verification_Collection_Id_msgid',     'Id-' )" />
                                    <th class="sortable" tal:content="python:here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_Verification_Collection_Title_msgid',  'Title-' )" />
                                    <th class="sortable" tal:content="python:here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_Verification_Collection_AcquireRoleAssignments_msgid',  'Acquire Roles-' )" />
                                    <th class="sortable" tal:content="python:here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_Verification_Collection_AcquireRoleAssignmentsSuccess_msgid',  'Acquire Roles Success-' )" />
                                    <th class="sortable" tal:content="python:here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_Verification_Collection_AcquireRoleAssignmentsStatus_msgid',  'Acquire Roles Status-' )" />
                                </tr>
                            </thead>
                            <tbody>
                                <tr tal:repeat="pCollection pCollections" >
                                    <td tal:define="pCollectionSuccess python: pCollection.get( 'success', False)">
                                        <font tal:condition="pCollectionSuccess" color="green">
                                            <strong tal:content="python: here.fTranslateI18N(  'ModelDDvlPlone', 'ModelDDvlPlone_True',   'True-' )" />
                                        </font>
                                        <font tal:condition="not: pCollectionSuccess" color="red">
                                            <strong tal:content="python: here.fTranslateI18N(  'ModelDDvlPlone', 'ModelDDvlPlone_False',  'False-' )" />
                                        </font>
                                    </td>
                                    <td tal:content="python: pCollection.get( 'status', '')" />
                                    <td tal:condition="pIsInitialization"  tal:content="python: pCollection.get( 'committed', '')" />
                                    <td tal:content="python: pCollection.get( 'tipo_coleccion', '')" />
                                    <td tal:content="python: pCollection.get( 'id_coleccion', '')" />
                                    <td tal:content="python: pCollection.get( 'titulo_coleccion', '')" />
                                    <td tal:content="python: pCollection.get( 'acquire_role_assignments', True)" />
                                    <td tal:content="python: pCollection.get( 'acquire_role_assignments_success', False)" />
                                    <td tal:content="python: pCollection.get( 'acquire_role_assignments_status', '')" />                                    
                                </tr>
                            </tbody>
                        </table>
                        <br/>
                        <br/>
                    </tal:block>
                </tal:block>                            
            </tal:block>
        </tal:block>
        <br/>
    </div>
    
    
        
    
    
    
    

   
    <div metal:define-macro="tTRAVerificationOrInitializationReport_Singletons_i18n" >
        
        <tal:block tal:define="
            pVerifyOrInitResult_Singletons          pVerifyOrInitResult_Singletons | nothing;
            pOverallSuccess                         python: pVerifyOrInitResult_Singletons and pVerifyOrInitResult_Singletons.get( 'success', False);
            pVerifyOrInitActionTitle                python: u'%s %s %s' % ( pVerifyOrInitAction, pVerifyOrInitName, here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_VerifyOrInitializeSectionTitle_Singletons_msgid', 'Singletons',),)" >
       
            <h3>
                <span tal:content="pVerifyOrInitActionTitle" />
                &nbsp;
                <span tal:condition="not: pVerifyOrInitResult_Singletons" >
                    <font  color="red">
                        <strong  tal:content="python: u'%s %s' % ( pVerifyOrInitActionTitle, here.fTranslateI18N(  'ModelDDvlPlone', 'ModelDDvlPlone_Error_NoResult_msgid',   'No result obtained -' ),)" />
                    </font>
                </span>
                <tal:block tal:condition="pVerifyOrInitResult_Singletons" >
                    <font tal:condition="pOverallSuccess"  color="green">
                       <strong tal:content="python: here.fTranslateI18N(  'ModelDDvlPlone', 'ModelDDvlPlone_Success_msgid',   'Success-' )" />
                    </font>
                    <font tal:condition="not: pOverallSuccess"  color="red">
                       <strong tal:content="python:  here.fTranslateI18N(  'ModelDDvlPlone', 'ModelDDvlPlone_Failure_msgid',   'Failure-' )" />
                    </font>
                </tal:block>    
            </h3>

            <tal:block tal:condition="pVerifyOrInitResult_Singletons" >
            
            
                <table class="listing" tal:define="pClasesFilas python: [ 'even', 'odd',]; global pRowIndex python: 0" >
                    <thead>
                        <tr>
                            <th colspan="2" class="nosort"/>
                        </tr>
                    </thead>
                    <tbody>
                        <tr  class="#" tal:define="global pRowIndex python: pRowIndex + 1"  tal:attributes="class python: pClasesFilas[ pRowIndex % 2]" >
                            <td class="nosort">
                                <strong tal:content="python:  here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_VerifyOrInitialize_Container_MetaType_Label',   'Container Type-' )" />
                            </td>
                            <td tal:content="python: pVerifyOrInitResult_Singletons.get( 'container_type', '')" />
                        </tr>
                        <tr  class="#" tal:define="global pRowIndex python: pRowIndex + 1"  tal:attributes="class python: pClasesFilas[ pRowIndex % 2]" >
                            <td>
                                <strong tal:content="python: here.fTranslateI18N(  'gvSIGi18n', 'gvSIGi18n_VerifyOrInitialize_Container_Title_Label',   'Container Title-' )" />
                            </td>
