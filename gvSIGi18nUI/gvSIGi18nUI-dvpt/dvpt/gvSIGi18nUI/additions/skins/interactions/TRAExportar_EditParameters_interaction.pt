                                                    id python: 'theExportFormat_%s' % aExportFormatOption;
                                                    value aExportFormatOption" />
                                        </tal:block>
                                        <strong tal:content="python: here.TRAgvSIGi18n_tool.fTranslateI18N( here, 'gvSIGi18n', aExportFormatOptionMsgId,   aExportFormatOption + '-' )" />
                                        
