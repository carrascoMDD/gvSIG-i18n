                                        aExportFormatOption      python: pExportFormatVocabulary[ aExportFormatOptionIndex];
                                        aExportFormatOptionMsgId python: pExportFormatVocabularyMsgIds[ aExportFormatOptionIndex];
                                        onchangehandler          python: 'pTRASelectEncodingToExportLanguage( %c%s%c)' % ( 39, aExportFormatOption, 39, );">
                                        <tal:block tal:condition="python: pExportFormat == aExportFormatOption" >
                                            <input checked="on"  type="radio"  class="noborder" 
                                                onchange="#"
                                                name="theExportFormat" id="#"  value="#"  
                                                tal:attributes="
                                                    onchange onchangehandler;
                                                    id python: 'theExportFormat_%s' % aExportFormatOption;
                                                    value aExportFormatOption" />
                                        </tal:block>
                                        <tal:block tal:condition="python: not( pExportFormat == aExportFormatOption)" >
                                            <input   type="radio"  class="noborder" 
                                                onchange="#"
                                                name="theExportFormat" id="#"  value="#"  
                                                tal:attributes="
