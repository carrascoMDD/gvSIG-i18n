                        <tr class="#" tal:attributes="class python: pClasesFilas[unIndexClassFila % 2]" >
                            <td colspan="2" align="left" valign="baseline">
                                <p class="formHelp"  tal:content="python: here.TRAgvSIGi18n_tool.fTranslateI18N( here, 'gvSIGi18n', 'gvSIGi18n_exportar_Parameter_IncludeLocalesCSV_help',   'Include in the downloadable archive, a locales.csv file declaring the translations files contained in the archive (used by gvSIG).-' )" />
                            </td>
                        </tr>                  

                        
                        
                        
                             
                        <tr class="#" tal:attributes="class python: pClasesFilas[unIndexClassFila % 2]" tal:define="global unIndexClassFila python: unIndexClassFila + 1">
