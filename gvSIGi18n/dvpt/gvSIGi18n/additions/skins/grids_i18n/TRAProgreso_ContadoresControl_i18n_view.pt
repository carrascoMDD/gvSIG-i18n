<div metal:define-macro="tProgreso_ContadoresControl_i18n" >

    <tal:block tal:define="pContadoresControl pContadoresControl | nothing" >
    
        <tal:block tal:condition="python: not pContadoresControl" >
            <p>
                <strong i18n:domain="gvSIGi18n"  i18n:translate="gvSIGi18n_nohaycontadorescontrol_warning_msgid">There are no control counters for the process available at this time-</strong>
            </p>
        </tal:block>
        
        <tal:block tal:condition="python: pContadoresControl" >
            <h3>
                <strong  tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_attr_contadoresControl_label', 'Control Counters-')" />
            </h3>
            <p class="formHelp" tal:content="python:  here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_attr_contadoresControl_help', 'Stores the counters during the long-lived process.-')" />
            <br/>

            <p>
                <span>
                    <strong  tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_Timestamp_msgid',  'Date and Time-' )" />
                </span>
                &nbsp;
                <span  tal:content="python: pContadoresControl.get( 'timestamp', '')" />
            </p>
            <p tal:condition="python: pContadoresControl.get( 'from_progress_handler', False)">
                <strong  tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_ProgressCounters_fromProgressHandler_msgid',  'Progress Counters from Progress Handler-' )" />
            </p>
            <p tal:condition="python: not pContadoresControl.get( 'from_progress_handler', False)">
                <strong  tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_ProgressCounters_fromPersistentProgress_msgid',  'Progress Counters from Persistent Progress-' )" />
            </p>
            <br/>

