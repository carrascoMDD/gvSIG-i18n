<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
    lang="en"
    metal:use-macro="here/main_template/macros/master"
    i18n:domain="plone">
<body>                     


<metal:main fill-slot="main">
    <tal:main-macro metal:define-macro="main"
        tal:define="text string:Catalogo de cadenas y sus traduciones a varios idiomas.
        global pStartTime       pStartTime | python: here.ModelDDvlPlone_tool.fMillisecondsNow()" 
        i18n:domain="gvSIGi18n">


           

        <tal:block tal:define="pExecutionRecord python: here.fStartExecution( 'template', 'TRACrearCadenas_action.pt', None, True, { 'log_what': 'details', 'log_when': True, });
            pPermissionsCache python: {};
            pRolesCache       python: {}" >

            <div metal:use-macro="here/document_actions/macros/document_actions">
                Document actions (print, sendto etc)
            </div>
        
            <h1 tal:content="object_title" class="documentFirstHeading">
                Title or id
            </h1>
        
            <div metal:use-macro="here/document_byline/macros/byline">
                Get the byline - contains details about author and modification date.
            </div>
    
            <tal:block tal:condition="python: not here.getCatalogo().fAllowWrite()" >
                <div class="portalMessage" >
                    <span tal:content="python: here.ModelDDvlPlone_tool.fTranslateI18N( here, 'gvSIGi18n', 'gvSIGi18n_TranslationsCatalogIsLockedAgainstModifications', 'The Translations Catalog Is Locked Against Modifications')" />
                </div>
            </tal:block>

 
            <metal:block metal:use-macro="here/PerformanceAnalysis/macros/tSetupPerformanceAnalysis" />   

            <tal:block tal:define="
                pTrue                python: True;
                pFalse               python: False;
                pRetrievalStartTime  python: here.ModelDDvlPlone_tool.fMillisecondsNow();
                pProfilingResults python: (pPerformanceAnalysis or {}).get( 'profiling_results', None);
                unosArgs python: { 
                    'theTimeProfilingResults'     :pProfilingResults,
                    'theElement'                  :here, 
                    'theParent'                   :None,
                    'theParentTraversalName'      :'',
                    'theTypeConfig'               :None, 
                    'theAllTypeConfigs'           :None, 
                    'theViewName'                 :'Tabular', 
                    'theRetrievalExtents'         :[ 'traversals', 'owner', 'cursor', 'extra_links',],
                    'theWritePermissions'         :[  ],
                    'theFeatureFilters'           :None, 
                    'theInstanceFilters'          :None,
                    'theTranslationsCaches'       :None,
                    'theCheckedPermissionsCache'  :pPermissionsCache,
                    'theAdditionalParams'         :None, };    
                SRES  python: here.ModelDDvlPlone_tool.fRetrieveTypeConfig( **unosArgs);
                pRetrievalEndTime  python: here.ModelDDvlPlone_tool.fMillisecondsNow();
                pPermiteEditar  python: SRES[ 'read_permission'] and SRES[ 'write_permission']; 
                pPermiteEliminar python: SRES[ 'read_permission'] and SRES[ 'write_permission'] and SRES[ 'delete_permission'] and SRES[ 'container_element'][ 'read_permission'] and SRES[ 'container_element'][ 'write_permission']; 
                pShowOrHideTrees pTrue; 
                pMostrarAnteriorPosterior pTrue;
                pAllowTextualAndTabular pTrue;
                pAllowRefrescar pTrue;
                pPaginaRefrescar string:Tabular" >




                <metal:block metal:use-macro="here/PerformanceAnalysis/macros/tRenderRetrievalTime_HTML" />   
            
                <tal:block tal:condition="python: not here.getCatalogo().fAllowWrite()" >
                    <div class="portalMessage" >
                        <span tal:content="python: here.ModelDDvlPlone_tool.fTranslateI18N( here, 'gvSIGi18n', 'gvSIGi18n_TranslationsCatalogIsLockedAgainstModifications', 'The Translations Catalog Is Locked Against Modifications')" />
                    </div>
                </tal:block>

          
                <metal:block metal:use-macro="here/CabeceraEnTabla_i18n_view/macros/tCabeceraEnTabla_i18n" />   
    
                <br/>
    
                
                <tal:block tal:define="
                    pCrearCadenasUseCaseAssessmentResult python: here.fUseCaseAssessment( 
                        theUseCaseName          = 'Create_TRACadena', 
                        theElementsBindings     = { 'object': here,},
                        theRulesToCollect       = None, 
                        thePermissionsCache     = pPermissionsCache, 
                        theRolesCache           = pRolesCache, 
                        theParentExecutionRecord= pExecutionRecord);
                    pLimpiarCadenasUseCaseAssessmentResult python: here.fUseCaseAssessment( 
                        theUseCaseName          = 'Cleanup_TRAColeccionSolicitudesCadenas', 
                        theElementsBindings     = { 'object': here,},
                        theRulesToCollect       = None, 
                        thePermissionsCache     = pPermissionsCache, 
                        theRolesCache           = pRolesCache, 
                        theParentExecutionRecord= pExecutionRecord);
                    pPuedeCrearYLimpiarCadenas python: pCrearCadenasUseCaseAssessmentResult and pCrearCadenasUseCaseAssessmentResult.get( 'success', False) and pLimpiarCadenasUseCaseAssessmentResult and pLimpiarCadenasUseCaseAssessmentResult.get( 'success', False)" >
                    <tal:block tal:condition="pPuedeCrearYLimpiarCadenas">  
                    
                    
                    
                        <tal:block tal:define="
                            pConfirmMagicMilliseconds python: request.get( 'theConfirm', '');
                            pIsAcceptableMagicMilliseconds python: here.fIsAcceptableMagicMilliseconds ( pConfirmMagicMilliseconds, here.getCatalogo().getSegundosParaConfirmarAccion() or 120)" >
                            <tal:block tal:condition="not: pIsAcceptableMagicMilliseconds">
                                <p><font size="3" color="red"><strong tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_demasiadaEsperaVuelvaASolicitarCreacionCadenas_help', 'You have waited too long to confirm the creation of the requested new strings. Please, request the operation again if you wish to do so.-')" /></font>                 
                                <br/>
                                <p>
                                    <font size="3">
                                        <strong>
                                            <a href="#" title="#"  
                                                tal:define="aLinkTitle python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_cancelarCrearCadenas_msgid',   'Cancel creation of requested new strings-' )"
                                                tal:attributes="
                                                    title aLinkTitle;
                                                    href python: '%s/Tabular/' % here.absolute_url()">
                                                <span tal:content="aLinkTitle" />
                                            </a>
                                        </strong>
                                    </font>
                                </p>
                                <br/>
                            </tal:block>
                            <tal:block tal:condition="pIsAcceptableMagicMilliseconds">
                    
                                 <tal:block tal:define="pCrearResult python:here.fCrearImportarYLimpiarCadenas()" >
                                    <tal:block tal:condition="python: pCrearResult and ( pCrearResult[ 'effect'] == 'created')" >
                                        <tal:block tal:define="
                                            dummy python:request.response.redirect( '%sTabular/' % pCrearResult[ 'new_object_result'][ 'url']);" />
                                    </tal:block>
                                    <tal:block tal:condition="python: not ( pCrearResult) or not ( pCrearResult[ 'effect'] == 'created')" >
                                        <h4>
                                            <font color="Red">
                                                <span i18n:domain="ModelDDvlPlone"  i18n:translate="ModelDDvlPlone_nosehacreadoelemento_info_msgid" >ModelDDvlPlone_nosehacreadoelemento_info_msgid</span>
                                            </font>
                                        </h4>
                                        <p>
                                            <font color="Red">
                                                <span tal:content="python: pCrearResult.get( 'failure', '')"/>
                                            </font>
                                        </p>
                                        <br/>
                                    </tal:block>
                                </tal:block>
                            </tal:block>
                        </tal:block>
                    </tal:block>
                    
                    <tal:block tal:condition="not: pPuedeCrearYLimpiarCadenas">    
                        <br/>
                        <br/>
                        <a class="state-visible" href="#" title="#" 
                            tal:define="aLinkTitle python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_userCanNotCreateStrings_msgid',   'You are not allowed to create requested new strings.-' )"
                            tal:attributes="
                                title aLinkTitle;
                                href python: '%s/Tabular/' % here.absolute_url()">
                            <font size="3" >
                                <strong>
                                    <span tal:content="aLinkTitle" />
                                </strong>
                            </font>
                        </a>
                    </tal:block>
                    <br/>
                    <br/>
                    
                </tal:block>
            </tal:block>
        </tal:block>

        <div tal:replace="structure python: here.ModelDDvlPlone_tool.fRenderTemplate( here, '%sFooter_view' )" />


    </tal:main-macro>
</metal:main>

</body>
</html>


