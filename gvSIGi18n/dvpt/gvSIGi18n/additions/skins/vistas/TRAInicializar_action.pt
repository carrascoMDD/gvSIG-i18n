<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="plone">
<body>                     

                             
<metal:main fill-slot="main">
    <tal:main-macro metal:define-macro="main"
        tal:define="text string:Catalogo de cadenas y sus traduciones a varios idiomas.;
        global pStartTime       pStartTime | python: here.fMillisecondsNow()" 
        i18n:domain="gvSIGi18n">

           


        <tal:block tal:define="
            pExecutionRecord python: here.fStartExecution( 'template', 'TRAInicializar_action.pt', None, True, { 'log_what': 'details', 'log_when': True, });
            pPermissionsCache python: {};
            pRolesCache python: {}" >
            
            <div metal:use-macro="here/document_actions/macros/document_actions">
                Document actions (print, sendto etc)
            </div>
    
            <h1 tal:content="object_title" class="documentFirstHeading">
              Title or id
            </h1>
    
            <div metal:use-macro="here/document_byline/macros/byline">
              Get the byline - contains details about author and modification date.
            </div>
            
            
            <tal:block tal:condition="python: not here.fAllowWrite()" >
                <div class="portalMessage" >
                    <span tal:content="python: here.fTranslateI18N( here, 'gvSIGi18n', 'gvSIGi18n_TranslationsCatalogIsLockedAgainstModifications', 'The Translations Catalog Is Locked Against Modifications')" />
                </div>
            </tal:block>
                
            
    
            <metal:block metal:use-macro="here/TRACabeceraEnTabla_i18n_view/macros/tCabeceraEnTabla_i18n" />   
                
            <tal:block tal:define="
                pRefreshPageName   string:;
                pShowActivityLink  python: True;
                pShowReportLink    python: True;
                pShowDetailsLink   python: True;
                pShowRootLink      python: True;
                ">
                <metal:block metal:use-macro="here/TRACatalogo_inner/macros/tTRACatalogoLinksMenu" />
            </tal:block>
            
            <h2>
                <strong  tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_InitializeTranslationsCatalog',  'Initialize the Translations Catalog-' )" />
                &ensp;
                <strong  tal:content="python: here.getCatalogo().Title()" />
            </h2>
            
            

            
            <tal:block tal:define="
                pInicializarUseCaseAssessmentResult python: here.fUseCaseAssessment( 
                    theUseCaseName          = 'Initialize_TRACatalogo', 
                    theElementsBindings     = { 'object': here,},
                    theRulesToCollect       = None, 
                    thePermissionsCache     = pPermissionsCache, 
                    theRolesCache           = pRolesCache, 
                    theParentExecutionRecord= pExecutionRecord)" >
                    
                <tal:block tal:condition="python: not( pInicializarUseCaseAssessmentResult and pInicializarUseCaseAssessmentResult.get( 'success', False))">    
                    <br/>
                    <br/>
                    <font size="3" >
                        <strong>
                            <tal:block tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_userCanNotInitializeCatalogo_msgid',   'You are not allowed to verify the translations catalog.-' )" />
                        </strong>
                    </font>
                    <br/>
                    <br/>
                </tal:block>
                
                
                <tal:block tal:condition="python: pInicializarUseCaseAssessmentResult and pInicializarUseCaseAssessmentResult.get( 'success', False)">  
                
                    <tal:block tal:define="pConfirmMagicMilliseconds python: request.get( 'theConfirm', '')" >
                    
                        <tal:block tal:condition="not: pConfirmMagicMilliseconds" >

                            <a href="#" title="#" 
                               tal:define="aLinkTitle python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_confirmarInicializar_msgid',   'Confirm request to Initialize the Translations Catalog-' )"                                    
                               tal:attributes="
                                   title aLinkTitle;
                                   href python: '%s/TRAInicializar_action/?theConfirm=%s' % ( here.getCatalogo().absolute_url(), here.fMagicMillisecondsNowString())">
                                <font size="3" color="red">
                                    <strong>
                                        <span tal:content="aLinkTitle" />
                                    </strong>
                                </font>
                            </a>
                            &ensp;
                            <span tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_periodoParaConfirmarEnSegundos_msgid', 'Please confirm within the next (seconds)-')" />
                            <span tal:content="python: str( here.getSegundosParaConfirmarAccion() or 120)" />
                            <span tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_segundosPeriodoParaConfirmar_help', 'seconds')" />
                            <br/>
                            <p class="formHelp">
                                <span tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_confirmarInicializar_help', 'Confirm to Initialize the translations Catalog, possibly taking a long time and demanding significant server resources, depending on the number of elements found, with usually higher cost with higher number of strings and languages (as seen in the root or summary report pages).-')" />
                                <br/>
                                <span tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_confirmarTimeout_help', 'You must confirm within the allowed time, or  if you need more time to review the impact of the requested action, take your time to do so, and request the action again later.-')" />
                            </p>            
                            <br/>            
                            <br/>
                            <br/>                        
                        </tal:block>
                        
                        
                        <tal:block tal:condition="pConfirmMagicMilliseconds" >
             
            
                            <tal:block tal:define="pIsAcceptableMagicMilliseconds python: here.fIsAcceptableMagicMilliseconds ( pConfirmMagicMilliseconds, here.getSegundosParaConfirmarAccion() or 120)" >
                                
                                <tal:block tal:condition="not: pIsAcceptableMagicMilliseconds">
                                    <p><font size="3" color="red"><strong tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_demasiadaEsperaVuelvaASolicitarAccion_help', 'You have waited too long to confirm the action. Please, request the action again if you wish to do so.-')" /></font>                 
                                    <br/>
                                    <p>
                                        <font size="3">
                                            <strong>
                                                <a href="#" title="#"  
                                                    tal:define="aLinkTitle python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_cancelarAccion_msgid',   'Cancel action-' )"
                                                    tal:attributes="
                                                        title aLinkTitle;
                                                        href python: '%s/' % here.absolute_url()">
                                                    <span tal:content="aLinkTitle" />
                                                </a>
                                            </strong>
                                        </font>
                                    </p>
                                    <br/>
                                    <br/>
                                </tal:block>
                                
                                <tal:block tal:condition="pIsAcceptableMagicMilliseconds">
                
                                    <tal:block tal:define="pInicializationResult python: here.fVerifyOrInitialize(
                                        theAllowInitialization      =True,
                                        theCheckPermissions         =True,
                                        thePermissionsCache         =pPermissionsCache, 
                                        theRolesCache               =pRolesCache, 
                                        theParentExecutionRecord    =pExecutionRecord)" >
                                        <metal:block metal:use-macro="here/TRAVerificationReport_i18n_view/macros/tTRAInicializationReport_i18n" />
                                    </tal:block>
                                </tal:block>
                            </tal:block>
            
                            </tal:block>
                        </tal:block>
                    </tal:block>
                </tal:block>            
                                     
    
            <tal:block tal:define="dummy python: pExecutionRecord and here.pEndExecution( pExecutionRecord)" />
    
        
            <tal:block tal:replace="nothing">
                <metal:block metal:use-macro="here/TRAFooter_i18n_view/macros/tFooter_i18n" />
                <tal:block tal:replace="structure python: pExecutionRecord and here.TRARenderExecutionDetails( pExecutionRecord)" />
            </tal:block>

        </tal:block>
     </tal:main-macro>
</metal:main>

</body>
</html>
