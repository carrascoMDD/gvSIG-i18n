<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
<body>


<div metal:define-macro="tExportar_CatalogoLinks_i18n" >

    <table width="100%%" cellspacing="0" cellpadding="0" frame="void" >
        <tr>
            <td align="left" valign="baseline" >
                <font size="2">
                    <em><span tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRACatalogo_attr_nombreProducto_label',   'Product Name-' )" /></em>
                 </font>        
                <font size="3">
                    <strong><span tal:content="python: here.fAsUnicode( here.getNombreProducto())" /></strong>
                 </font>
            </td>         
            <td  aligh="center" valign="baseline">
                <tal:block tal:define="pUseCaseAssessmentResult python: here.fUseCaseAssessment(
                    theUseCaseName          = 'View_any_TRA_element', 
                    theElementsBindings     = { 'object': here.getCatalogo(),},
                    theRulesToCollect       = None, 
                    thePermissionsCache     = pPermissionsCache, 
                    theRolesCache           = pRolesCache, 
                    theParentExecutionRecord= pExecutionRecord);
                    pUseCaseAssessmentResultsCache pUseCaseAssessmentResultsCache | python: { };
                    void python: pUseCaseAssessmentResultsCache.update({ 'View_any_TRA_element':  pUseCaseAssessmentResult,})" 
                    tal:condition="python: pUseCaseAssessmentResult and pUseCaseAssessmentResult.get( 'success', False)">
                    <strong>
                       <a href="#" title="Tabular"  tal:attributes="href python: '%s/Tabular/' % here.absolute_url()">
                           <span tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_vistaAvanzada_msgid',   'Advanced view-' )" />
                       </a>
                   </strong>
                </tal:block>
            </td>
            <td align="right" valign="baseline">                    
                <strong>
                    <a href="#" title="Mostrar Informe"  tal:attributes="href python: '%s/TRACatalogoInforme/' % here.absolute_url()">
                        <span tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_mostrarinformedeestado_msgid',   'Show Status Report-' )" />
                    </a>
                </strong>
            </td>
        </tr>
    </table>
                    
</div>        
 
 
</body>
</html>

