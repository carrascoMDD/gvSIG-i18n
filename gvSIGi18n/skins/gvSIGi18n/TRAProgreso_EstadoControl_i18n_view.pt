      lang="en">
<body>



<div metal:define-macro="tProgreso_EstadoControl_i18n" >

    <tal:block tal:define="pEstadoControl pEstadoControl | nothing" >
    
        <tal:block tal:condition="python: not pEstadoControl" >
            <p>
                <strong i18n:domain="gvSIGi18n"  i18n:translate="gvSIGi18n_nohayEstadoControl_warning_msgid">There is no progress control status for the process available at this time-</strong>
            </p>
        </tal:block>
        
        <tal:block tal:condition="python: pEstadoControl" >
            <h3>
                <strong  tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_attr_estadoControl_label', 'Progress Control Status-')" />
            </h3>
            <p class="formHelp" tal:content="python:  here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_attr_estadoControl_help', 'Summary of the state of progress control for this long-lived process.-')" />
            
            <br/>
            <p>
                <span>
                    <strong  tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_Timestamp_msgid',  'Date and Time-' )" />
                </span>
                &nbsp;
                <span  tal:content="python: pEstadoControl.get( 'timestamp', '')" />
            </p>
            <br/>
            
            <table id="cid_TRAProgress_ControlStatus" class="listing" tal:define="pClasesFilas python: [ 'even', 'odd',]" >
                <thead>
                    <tr>
                        <th colspan="2" align="left"  class="nosort">
                            <font size="2">
                                <strong tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_attr_estadoControl_label', 'Progress Control Status-')" />
                            </font>
                        </th>
                    </tr>
                </thead>
                
                <tbody tal:define="global pRowIndex python: 0;" >
                    <tr class="#" tal:define="global pRowIndex python: pRowIndex + 1"  tal:attributes="class python: pClasesFilas[ pRowIndex % 2]">
                        <td tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_estadoControl_started_label', 'started')"/>
                        <td align="right" tal:content="python:'%s' % pEstadoControl.get( 'started', False)" />
                    </tr>
                    <tr class="#" tal:define="global pRowIndex python: pRowIndex + 1"  tal:attributes="class python: pClasesFilas[ pRowIndex % 2]">
                        <td tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_estadoControl_paused_label', 'paused')"/>
                        <td align="right" tal:content="python:'%s' % pEstadoControl.get( 'paused', False)" />
                    </tr>
                    <tr class="#" tal:define="global pRowIndex python: pRowIndex + 1"  tal:attributes="class python: pClasesFilas[ pRowIndex % 2]">
                        <td tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_estadoControl_completed_label', 'completed')"/>
                        <td align="right" tal:content="python:'%s' % pEstadoControl.get( 'completed', False)" />
                    </tr>
                    <tr class="#" tal:define="global pRowIndex python: pRowIndex + 1"  tal:attributes="class python: pClasesFilas[ pRowIndex % 2]">
                        <td tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_estadoControl_terminated_label', 'terminated')"/>
                        <td align="right" tal:content="python:'%s' % pEstadoControl.get( 'terminated', False)" />
                    </tr>
                    <tr class="#" tal:define="global pRowIndex python: pRowIndex + 1"  tal:attributes="class python: pClasesFilas[ pRowIndex % 2]">
                        <td tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_estadoControl_exception_label', 'exception')"/>
                        <td align="right" tal:content="python:'%s' % pEstadoControl.get( 'exception', False)" />
                    </tr>
                </tbody>
            </table>
            <br/>               
            <br/>
                            
                
            <tal:block tal:define="pPendingControlRequests python: pEstadoControl.get( 'pending_control_requests', [])" >
                <tal:block tal:condition="not: pPendingControlRequests" >
                    <p tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_estadoControl_NoPendingControlRequests_label', 'There are NO Pending Control Requests-')" />
                </tal:block>
            
                <tal:block tal:condition="pPendingControlRequests" >
                
                    <h3>
                        <strong  tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_estadoControl_pendingControlRequests_label', 'Pending Control Requests-')" />
                    </h3>
                    <p class="formHelp" tal:content="python:  here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_estadoControl_pendingControlRequests_help', 'Control requests not yet accepted by the long-lived process.-')" />
                    
                    
                    <table id="cid_TRAProgress_PendingControlRequests" class="listing" tal:define="pClasesFilas python: [ 'even', 'odd',]" >
                        <thead>
                            <tr>
                                <th align="left"  class="sortable">
                                    <strong tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_ControlRequest_Action_label', 'Requested Action-')" />
                                </th>
                                <th align="left"  class="sortable">
                                    <strong tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_ControlRequest_RequestingUser_label', 'Requesting User-')" />
                                </th>
                                <th align="left"  class="sortable">
                                    <strong tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_ControlRequest_RequestTime_label', 'Request Time-')" />
                                </th>
                            </tr>
                        </thead>
                        <tbody tal:define="global pRowIndex python: 0;" >
                            <tal:block tal:repeat="pPendingControlRequest pPendingControlRequests" >
                                <tr class="#" tal:define="global pRowIndex python: pRowIndex + 1"  tal:attributes="class python: pClasesFilas[ pRowIndex % 2]">
                                    <td align="left" tal:content="python: pPendingControlRequest.get( 'action', '')"/>
                                    <td align="left" tal:content="python: pPendingControlRequest.get( 'member_id', '')"/>
                                    <td align="left" tal:content="python: pPendingControlRequest.get( 'requested_timestamp', '')"/>
                                </tr>
                                
                            </tal:block>
                        </tbody>
                    </table>
                    <br/>
                    <br/>
                </tal:block>
            </tal:block>

            
            
            
                           
                
            <tal:block tal:define="pPerformedControlRequests python: pEstadoControl.get( 'performed_control_requests', [])" >
                <tal:block tal:condition="not: pPerformedControlRequests" >
                    <p tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_estadoControl_NoPerformedControlRequests_label', 'There are NO Performed Control Requests-')" />
                </tal:block>

                <tal:block tal:condition="pPerformedControlRequests" >
                
                    <h3>
                        <strong  tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_estadoControl_performedControlRequests_label', 'Performed Control Requests-')" />
                    </h3>
                    <p class="formHelp" tal:content="python:  here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_estadoControl_performedControlRequests_help', 'Control requests already accepted by the long-lived process.-')" />
                    
                    
                    <table id="cid_TRAProgress_PerformedControlRequests" class="listing" tal:define="pClasesFilas python: [ 'even', 'odd',]" >
                        <thead>
                            <tr>
                                <th align="left"  class="sortable">
                                    <strong tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_ControlRequest_Action_label', 'Performed Action-')" />
                                </th>
                                <th align="left"  class="sortable">
                                    <strong tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_ControlRequest_RequestingUser_label', 'Requesting User-')" />
                                </th>
                                <th align="left"  class="sortable">
                                    <strong tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_ControlRequest_RequestedTime_label', 'Requested Time-')" />
                                </th>
                                <th align="left"  class="sortable">
                                    <strong tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_ControlRequest_PerformedTime_label', 'Performed Time-')" />
                                </th>
                                <th align="left"  class="sortable">
                                    <strong tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_ControlRequest_ChangesApplied_label', 'Changes Applied-')" />
                                </th>
                            </tr>
                        </thead>
                        <tbody tal:define="global pRowIndex python: 0;" >
                            <tal:block tal:repeat="pPerformedControlRequest pPerformedControlRequests" >
                                <tal:block tal:condition="python: pPerformedControlRequest.get( 'changes_applied', [])" >
                                    <tr class="#" tal:define="global pRowIndex python: pRowIndex + 1"  tal:attributes="class python: pClasesFilas[ pRowIndex % 2]">
                                        <td align="left" tal:content="python: pPerformedControlRequest.get( 'action', '')"/>
                                        <td align="left" tal:content="python: pPerformedControlRequest.get( 'member_id', '')"/>
                                        <td align="left" tal:content="python: pPerformedControlRequest.get( 'requested_timestamp', '')"/>
                                        <td align="left" tal:content="python: pPerformedControlRequest.get( 'performed_timestamp', '')"/>
                                        <td align="left">
                                            <tal:block tal:repeat="pChangeApplied python: pPerformedControlRequest.get( 'changes_applied', [])" >
                                                <span tal:content="python: '%s' % pChangeApplied" />
                                                <br/>
                                            </tal:block>
                                        </td>
                                    </tr>
                                </tal:block>
                                
                            </tal:block>
                        </tbody>
                    </table>
                    <br/>
                    <br/>
                </tal:block>
            </tal:block>

            
            
            
            
            
        </tal:block>
     </tal:block>

</div>








</body>
</html>
