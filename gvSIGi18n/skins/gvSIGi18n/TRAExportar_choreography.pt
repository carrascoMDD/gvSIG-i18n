        pExecutionRecord        python: here.fStartExecution( 'macro_interaction', 'TRAExportar_choreography/tExportar_ReviewParameters_choreography', pParentExecutionRecord, False);
        pPermissionsCache       pPermissionsCache | python: { };
        pRolesCache             pRolesCache       | python: { };
        pUseCaseAssessmentResultsCache pUseCaseAssessmentResultsCache | python: { };
