    <div metal:define-macro="tImportacionProgresoInforme_i18n" >

    
        <tal:block tal:replace="nothing">
        # ###############################################
        """Visual rendering component for an import process progress or final report.
        
        """
        </tal:block>

        
        <tal:block tal:define="
            global unIndexClassFila python: 0; 
            global unasClasesFilas  python: ('odd','even')" />
                
        <tal:block tal:define="pInforme pInforme | nothing" tal:condition="pInforme" >
            
            <br/>
            <tal:block tal:condition="python: pInforme.get( 'fecha_informe', '')">
                <font size="2">
                    <strong>
                        <span tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_FechaInforme_label', 'Report date and time-' )"/>
                        <span tal:content="python: str( pInforme.get( 'fecha_informe', ''))" />
                    </strong>
                </font>
                <br/>
            </tal:block>
            
            <tal:block tal:condition="python: pInforme.get( 'start_date', '')">
                <font size="1">
                    <strong>
                        <span tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_FechaComienzo_label', 'Start date and time-' )"/>
                        <span tal:content="python: str( pInforme.get( 'start_date', ''))" />
                    </strong>
                </font>
                <br/>
            </tal:block>

           <tal:block tal:condition="python: pInforme.get( 'end_date', '')">
                <font size="1">
                    <strong>
                        <span tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_FechaFin_label', 'End date and time-' )"/>
                        <span tal:content="python: str( pInforme.get(  'end_date', ''))" />
                    </strong>
                </font>
                <br/>
            </tal:block>

            <tal:block tal:condition="pInforme/error | nothing" >
                <font color="Red" size="3"><strong tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_Error_label', 'Error-' )"/></font>
                <br/>
                <font color="Red" size="2"><strong tal:content="pInforme/error" /></font>
                <br/>
                <tal:block tal:condition="pInforme/error_detail | nothing" >
                    <font color="Red" size="1" tal:define="pErrorDetail pInforme/error_detail">
                        <strong>
                            <tal:block tal:repeat="pErrorLine python: pErrorDetail.splitlines()">
                                <span tal:content="pErrorLine" />
                                <br/>
                            </tal:block>
                        </strong>
                    </font>
                </tal:block>
                <br/>
            </tal:block>
            
            
                        
          
