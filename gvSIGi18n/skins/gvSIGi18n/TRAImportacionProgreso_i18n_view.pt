                        <td align="right" valign="baseline">
                            <tal:block tal:condition="python: pInforme.get( 'expected_operations', 0)" 
                                tal:content="python: '%d%%' % int( ( ( 0.0 + pInforme.get( 'operations_done', 0)) / pInforme.get( 'expected_operations', 0)) * 100)" />
                            <tal:block tal:condition="python: not( pInforme.get( 'expected_operations', 0))" tal:content="string:100%" /> 
