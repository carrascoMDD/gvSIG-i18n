<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="plone">
<body>                     
<metal:main fill-slot="main">
    <tal:main-macro metal:define-macro="main"
        tal:define="text string:Catalogo de cadenas y sus traduciones a varios idiomas." 
        i18n:domain="gvSIGi18n">

        <tal:block tal:define="pExecutionRecord python: here.fStartExecution( 'template', 'TRAInformeContenidoIntercambio_action', None, True, { 'log_what': 'details', 'log_when': True, })">
    
            <div metal:use-macro="here/document_actions/macros/document_actions">
                Document actions (print, sendto etc)
            </div>
    
            <h1 tal:content="object_title" class="documentFirstHeading">
              Title or id
            </h1>
    
            <div metal:use-macro="here/document_byline/macros/byline">
              Get the byline - contains details about author and modification date.
            </div>
            
            
     
            <tal:block tal:define="pPermiteEditar python: False; pPermiteEliminar python: False; pShowOrHideTrees python: False">
                <metal:block metal:use-macro="here/TRACabeceraEnTabla_i18n_view/macros/tCabeceraEnTabla_i18n" />   
            </tal:block>
    
            
            
            <h2>
                <span i18n:domain="gvSIGi18n" i18n:translate="gvSIGi18n_TRAImportacion_attr_contenidoIntercambioTraducciones_label">Translations interchange archive contents-</span>
                &ensp;
                &ensp;
                &ensp;
                &ensp;
                 <font size="2">
                    <a href="#" title="#" 
                        tal:define="aLinkTitle python: '%s' %  here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_vista_todos_label',   'View All-')"
                        tal:attributes="
                            title aLinkTitle;
                            href python: '%s/TRAInformeTodosContenidosIntercambio_action' % here.getContenedor().absolute_url()">
                        <span tal:content="aLinkTitle" />
                    </a>
                </font>
                &ensp;
                &ensp;
                &ensp;
                &ensp;
                 <font size="2">
                    <a href="#" title="#" 
                        tal:define="aLinkTitle python: '%s' %  here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_vista_sumario_label',   'Summary view-')"
                        tal:attributes="
                            title aLinkTitle;
                            href python: '%s/TRAInformeContenidosImportacion_action' % here.getContenedor().absolute_url()">
                        <span tal:content="aLinkTitle" />
                    </a>
                </font>
                &ensp;
                &ensp;
                &ensp;
                &ensp;
                 <font size="2">
                    <a href="#" title="#" 
                        tal:define="aLinkTitle python: '%s' % here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_vista_tabular_label',   'Tabular view-')"
                        tal:attributes="
                            title aLinkTitle;
                            href python: '%s/Tabular' % here.absolute_url()">
                        <span tal:content="aLinkTitle" />
                    </a>
                </font>
            </h2>
            <p class="formHelp" i18n:domain="gvSIGi18n"  i18n:translate="gvSIGi18n_TRAImportacion_attr_contenidoIntercambioTraducciones_help" />
         
            <tal:block tal:define="pInforme python: here.fInformeContenidoIntercambio( pExecutionRecord)">
                <tal:block tal:condition="not: pInforme">
                     <p i18n:domain="gvSIGi18n"  i18n:translate="gvSIGi18n_nohaycontenidointercambiotraducciones_warning_msgid">There is no valid translation interchange content-</p>
                </tal:block>
                <tal:block  tal:condition="pInforme" >
                    <metal:block metal:use-macro="here/TRAContenidoIntercambio_i18n_view/macros/tInformeContenidoIntercambio_i18n" />   
                </tal:block>
            </tal:block>
            <br/>
    
            
            
            <tal:block tal:define="dummy python: pExecutionRecord and here.pEndExecution( pExecutionRecord)" />
    
            
            
            <metal:block metal:use-macro="here/TRAFooter_i18n_view/macros/tFooter_i18n" />
    
            <tal:block tal:replace="structure python: pExecutionRecord and here.TRARenderExecutionDetails( pExecutionRecord)" />
            
        
         </tal:block>
        
     </tal:main-macro>
</metal:main>

</body>
</html>



