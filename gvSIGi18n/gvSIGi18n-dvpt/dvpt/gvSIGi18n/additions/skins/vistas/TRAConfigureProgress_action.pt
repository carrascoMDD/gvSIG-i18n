<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="plone">
<body>                     
<metal:main fill-slot="main">
    <tal:main-macro metal:define-macro="main"
        tal:define="text string:Catalogo de cadenas y sus traduciones a varios idiomas.
        global pStartTime       pStartTime | python: here.ModelDDvlPlone_tool.fMillisecondsNow()" 
        i18n:domain="gvSIGi18n">

           


        <tal:block tal:define="
            pExecutionRecord python: here.fStartExecution( 'template', 'TRAConfigureProgress_action.pt', None, True, { 'log_when': True, });
            pPermissionsCache python: {};
            pRolesCache python: {}" >
    
            <div metal:use-macro="here/document_actions/macros/document_actions">
                Document actions (print, sendto etc)
            </div>
    
            <h1 tal:content="object_title" class="documentFirstHeading">
              Title or id
            </h1>
    
            <div metal:use-macro="here/document_byline/macros/byline">
              Get the byline - contains details about author and modification date.
            </div>
            
            
            <tal:block tal:condition="python: not here.fAllowWrite()" >
                <div class="portalMessage" >
                    <span tal:content="python: here.ModelDDvlPlone_tool.fTranslateI18N( here, 'gvSIGi18n', 'gvSIGi18n_TranslationsCatalogIsLockedAgainstModifications', 'The Translations Catalog Is Locked Against Modifications')" />
                </div>
            </tal:block>
            
            <metal:block metal:use-macro="here/CabeceraEnTabla_i18n_view/macros/tCabeceraEnTabla_i18n" />   
              
                
            <table width="100%%" cellspacing="0" cellpadding="0" frame="void" >
                <tr>
                    <td width="10%"  align="left" valign="baseline" >
                        <font size="2">
                            <em><span tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRACatalogo_attr_nombreProducto_label',   'Product Name-' )" /></em>
                         </font>        
                        <font size="3">
                            <strong><span tal:content="python: here.fAsUnicode( here.getCatalogo().getNombreProducto())" /></strong>
                         </font>
                    </td>         
                    <td  width="15%" align="center" valign="baseline">
                        <tal:block tal:define="
                            pUseCaseAssessmentResult python: here.fUseCaseAssessment( 
                                theUseCaseName          = 'Advanced_View_on_any_TRA_element', 
                                theElementsBindings     = { 'object': here,},
                                theRulesToCollect       = None, 
                                thePermissionsCache     = pPermissionsCache, 
                                theRolesCache           = pRolesCache, 
                                theParentExecutionRecord= pExecutionRecord)"
                            tal:condition="python: pUseCaseAssessmentResult and pUseCaseAssessmentResult.get( 'success', False)">    
                            <strong>
                                <a href="#" title="#" 
                                    tal:define="aLinkTitle python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_vistaAvanzada_msgid',   'Details-')"
                                    tal:attributes="
                                        title aLinkTitle;
                                        href python: '%s/Tabular/' % here.absolute_url()">
                                    <img src="#" alt="#" title="#" 
                                         tal:attributes="src python: '%s/tabular.gif' % here.portal_url(); alt aLinkTitle; title aLinkTitle" 
                                         id="icon-tabular" />
                                    <span tal:content="aLinkTitle" />
                                </a>
                            </strong>
                        </tal:block>
                    </td>
                    <td width="15%" align="right" valign="baseline">                    
                        <strong>
                           <a href="#" title="#"
                               tal:define="aLinkTitle python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_refrescar_action_label',   'Refresh-' )"
                               tal:attributes="
                                   title aLinkTitle;
                                   href python: '%s/TRAConfigureProgress_action/' % here.absolute_url()">
                                <img src="#" alt="#" title="#" 
                                     tal:attributes="src python: '%s/refrescar.gif' % here.portal_url(); alt aLinkTitle; title aLinkTitle" 
                                     id="icon-refrescar" />
                               <span tal:content="aLinkTitle" />
                           </a>
                        </strong>
                    </td>
                    <td width="15%"  align="right" valign="baseline">                    
                        <strong>
                            <a href="#" title="#"  
                                tal:define="aLinkTitle python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_volveracatalogo_msgid',   'Back to catalog-' )"
                                tal:attributes="
                                    title aLinkTitle;
                                    href python: '%s/TRACatalogo/' % here.getCatalogo().absolute_url()">
                                <span tal:content="aLinkTitle" />
                            </a>
                        </strong>
                    </td>                
                    <td width="15%" align="right" valign="baseline">                    
                        <strong>
                            <a href="#" title="#" 
                                tal:define="aLinkTitle python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_ProgressResults_msgid',   'Progress Results-' )"                                    
                                tal:attributes="
                                    title aLinkTitle;
                                    href python: '%s/TRAProgressResults_action/' % here.absolute_url()">
                                <span tal:content="aLinkTitle" />
                            </a>
                        </strong>
                    </td>
                    <td width="15%" align="right" valign="baseline">                    
                        <strong>
                            <a href="#" title="#" 
                                tal:define="aLinkTitle python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_ControlProgress_msgid',   'Control Progress-' )"                                    
                                tal:attributes="
                                    title aLinkTitle;
                                    href python: '%s/TRAControlProgress_action/' % here.absolute_url()">
                                <span tal:content="aLinkTitle" />
                            </a>
                        </strong>
                    </td>
                    <td width="15%" align="right" valign="baseline"
                        tal:define="pElementoEspecificacionProceso python: here.fDeriveElementoEspecificacionProceso()"
                        tal:condition="python: not ( pElementoEspecificacionProceso == None)">                    
                        <strong>
                            <a href="#" title="#" 
                                tal:define="aLinkTitle python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_ProcessSpecificacionElement_msgid',   'Process specificacion element-' )"                                    
                                tal:attributes="
                                    title aLinkTitle;
                                    href python: '%s/Tabular/' % pElementoEspecificacionProceso.absolute_url()">
                                <span tal:content="aLinkTitle" />
                            </a>
                        </strong>
                    </td>
                </tr>
            </table>
                        
            <br/>
            <br/>
            
               
            <h2>
                <strong  tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_ConfigureProcessParameters',  'Configure progress control parameters of long-lived process-' )" />
                <br/>
                <strong  tal:content="python: here.Title()" />
            </h2>
            <p>
                <strong  tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_ElementPath_msgid',  'Element path-' )" />
                <span tal:content="python: here.fPhysicalPathString()" />
            </p>
            
            <tal:block tal:define="
                pConfigureUseCaseAssessmentResult python: here.fUseCaseAssessment( 
                    theUseCaseName          = 'Configure_TRAProgreso', 
                    theElementsBindings     = { 'object': here,},
                    theRulesToCollect       = None, 
                    thePermissionsCache     = pPermissionsCache, 
                    theRolesCache           = pRolesCache, 
                    theParentExecutionRecord= pExecutionRecord)" >
                    
                <tal:block tal:condition="python: not( pConfigureUseCaseAssessmentResult and pConfigureUseCaseAssessmentResult.get( 'success', False))">    
                    <br/>
                    <br/>
                    <font size="3" >
                        <strong>
                            <tal:block tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_userCanNotConfigureProcessParameters_msgid',   'You are not allowed to configure progress control parameters for this long-lived process.-' )" />
                        </strong>
                    </font>
                    <br/>
                    <br/>
                </tal:block>
                
                
                <tal:block tal:condition="python: pConfigureUseCaseAssessmentResult and pConfigureUseCaseAssessmentResult.get( 'success', False)">    
                    
                    
                    <tal:block tal:define="pNuevosParametrosControl python: here.fNewVoidProgressControlParms_ToChange()" >

                        <tal:block tal:condition="python: request.form.get( 'theRunAfterPrevious', '') == 'Yes'" >
                            <tal:block tal:define="void python: pNuevosParametrosControl.update( { 'RunAfterPrevious':  True,})" />
                        </tal:block>
                        <tal:block tal:condition="python: request.form.get( 'theRunAfterPrevious', '') == 'No'" >
                            <tal:block tal:define="void python: pNuevosParametrosControl.update( { 'RunAfterPrevious':  False,})" />
                        </tal:block>
                    
                        <tal:block tal:define="pNuevosParametrosControl_StoreResults python: pNuevosParametrosControl.get( 'StoreResults', {})" >
                            <tal:block tal:condition="python: request.form.get( 'theStoreResults_Enabled', '') == 'Enabled'" >
                                <tal:block tal:define="void python: pNuevosParametrosControl_StoreResults.update( { 'enabled':  True,})" />
                            </tal:block>
                            <tal:block tal:condition="python: request.form.get( 'theStoreResults_Enabled', '') == 'Disabled'" >
                                <tal:block tal:define="void python: pNuevosParametrosControl_StoreResults.update( { 'enabled':  False,})" />
                            </tal:block>
                            <tal:block tal:condition="python: ( not ( request.form.get( 'theStoreResults_Enabled', '') == 'Enabled'))  and ( not ( request.form.get( 'theStoreResults_Enabled', '') == 'Disabled')) " >
                                <tal:block tal:define="void python: pNuevosParametrosControl_StoreResults.update( { 'enabled':  None,})" />
                            </tal:block>
                            <tal:block tal:define="void python: pNuevosParametrosControl_StoreResults.update( { 'max_milliseconds':       request.form.get( 'theStoreResults_max_milliseconds', ''),})" />
                            <tal:block tal:define="void python: pNuevosParametrosControl_StoreResults.update( { 'max_elements_traversed': request.form.get( 'theStoreResults_max_elements_traversed', ''),})" />
                            <tal:block tal:define="void python: pNuevosParametrosControl_StoreResults.update( { 'max_elements_changed':   request.form.get( 'theStoreResults_max_elements_changed', ''),})" />
                        </tal:block>
                    
                    
                        <tal:block tal:define="pNuevosParametrosControl_Transactional python: pNuevosParametrosControl.get( 'Transactional', {})" >
                            <tal:block tal:condition="python: request.form.get( 'theTransactional_Enabled', '') == 'Enabled'" >
                                <tal:block tal:define="void python: pNuevosParametrosControl_Transactional.update( { 'enabled':  True,})" />
                            </tal:block>
                            <tal:block tal:condition="python: request.form.get( 'theTransactional_Enabled', '') == 'Disabled'" >
                                <tal:block tal:define="void python: pNuevosParametrosControl_Transactional.update( { 'enabled':  False,})" />
                            </tal:block>
                            <tal:block tal:condition="python: ( not ( request.form.get( 'theTransactional_Enabled', '') == 'Enabled'))  and ( not ( request.form.get( 'theTransactional_Enabled', '') == 'Disabled')) " >
                                <tal:block tal:define="void python: pNuevosParametrosControl_Transactional.update( { 'enabled':  None,})" />
                            </tal:block>
                            <tal:block tal:define="void python: pNuevosParametrosControl_Transactional.update( { 'max_milliseconds':       request.form.get( 'theTransactional_max_milliseconds', ''),})" />
                            <tal:block tal:define="void python: pNuevosParametrosControl_Transactional.update( { 'max_elements_traversed': request.form.get( 'theTransactional_max_elements_traversed', ''),})" />
                            <tal:block tal:define="void python: pNuevosParametrosControl_Transactional.update( { 'max_elements_changed':   request.form.get( 'theTransactional_max_elements_changed', ''),})" />
                        </tal:block>
                    
                    
                        <tal:block tal:define="pNuevosParametrosControl_Logging python: pNuevosParametrosControl.get( 'Logging', {})" >
                            <tal:block tal:condition="python: request.form.get( 'theLogging_Enabled', '') == 'Enabled'" >
                                <tal:block tal:define="void python: pNuevosParametrosControl_Logging.update( { 'enabled':  True,})" />
                            </tal:block>
                            <tal:block tal:condition="python: request.form.get( 'theLogging_Enabled', '') == 'Disabled'" >
                                <tal:block tal:define="void python: pNuevosParametrosControl_Logging.update( { 'enabled':  False,})" />
                            </tal:block>
                            <tal:block tal:condition="python: ( not ( request.form.get( 'theLogging_Enabled', '') == 'Enabled'))  and ( not ( request.form.get( 'theLogging_Enabled', '') == 'Disabled')) " >
                                <tal:block tal:define="void python: pNuevosParametrosControl_Logging.update( { 'enabled':  None,})" />
                            </tal:block>
                            <tal:block tal:define="void python: pNuevosParametrosControl_Logging.update( { 'max_milliseconds':             request.form.get( 'theLogging_max_milliseconds', ''),})" />
                            <tal:block tal:define="void python: pNuevosParametrosControl_Logging.update( { 'max_elements_traversed':       request.form.get( 'theLogging_max_elements_traversed', ''),})" />
                            <tal:block tal:define="void python: pNuevosParametrosControl_Logging.update( { 'max_elements_changed':         request.form.get( 'theLogging_max_elements_changed', ''),})" />
                            <tal:block tal:define="void python: pNuevosParametrosControl_Logging.update( { 'log_every_nth_transactions':   request.form.get( 'theLogging_log_every_nth_transactions', ''),})" />
                        </tal:block>
                    
                    
                    
                        <tal:block tal:define="pNuevosParametrosControl_YieldProcessor python: pNuevosParametrosControl.get( 'YieldProcessor', {})" >
                            <tal:block tal:condition="python: request.form.get( 'theYieldProcessor_Enabled', '') == 'Enabled'" >
                                <tal:block tal:define="void python: pNuevosParametrosControl_YieldProcessor.update( { 'enabled':  True,})" />
                            </tal:block>
                            <tal:block tal:condition="python: request.form.get( 'theYieldProcessor_Enabled', '') == 'Disabled'" >
                                <tal:block tal:define="void python: pNuevosParametrosControl_YieldProcessor.update( { 'enabled':  False,})" />
                            </tal:block>
                            <tal:block tal:condition="python: ( not ( request.form.get( 'theYieldProcessor_Enabled', '') == 'Enabled'))  and ( not ( request.form.get( 'theYieldProcessor_Enabled', '') == 'Disabled')) " >
                                <tal:block tal:define="void python: pNuevosParametrosControl_YieldProcessor.update( { 'enabled':  None,})" />
                            </tal:block>
                            <tal:block tal:define="void python: pNuevosParametrosControl_YieldProcessor.update( { 'max_milliseconds':             request.form.get( 'theYieldProcessor_max_milliseconds', ''),})" />
                            <tal:block tal:define="void python: pNuevosParametrosControl_YieldProcessor.update( { 'max_elements_traversed':       request.form.get( 'theYieldProcessor_max_elements_traversed', ''),})" />
                            <tal:block tal:define="void python: pNuevosParametrosControl_YieldProcessor.update( { 'max_elements_changed':         request.form.get( 'theYieldProcessor_max_elements_changed', ''),})" />
                            <tal:block tal:define="void python: pNuevosParametrosControl_YieldProcessor.update( { 'percent_active_time':          request.form.get( 'theYieldProcessor_percent_active_time', ''),})" />
                        </tal:block>
                    
                        
                        <tal:block tal:define="
                            pServiceResult python: here.fService_ProcessControl( 
                                theProcessControlAction  = 'ChangeParameters',
                                theProgressHandlerKey = {   
                                    'translations_catalog_root_path':        here.fPathDelRaiz(),
                                    'progress_element_UID':                  here.UID(),
                                },
                                theAdditionalParms       =pNuevosParametrosControl,
                                thePermissionsCache      =pPermissionsCache,
                                theRolesCache            =pRolesCache,
                                theParentExecutionRecord =pExecutionRecord);
                            unServiceSuccess                python:  ( pServiceResult or {}).get( 'success', False);" >   
                            <p tal:content="python: 'Change Parameters success =%s' % unServiceSuccess" />
                        </tal:block>

                    </tal:block>
                    
                    
                    
                    
                    
                    
                    
                    <tal:block tal:define="pParametrosControl python: here.fParametrosControl()" >
                    
                        <tal:block tal:condition="not: pParametrosControl" >
                            <p>
                                <strong i18n:domain="gvSIGi18n"  i18n:translate="gvSIGi18n_nohayparametroscontrol_warning_msgid">There are no control parameters for the process available at this time.-</strong>
                            </p>
                            <br/>
                            <br/>                        
                        </tal:block>
                        
                        
                        <tal:block tal:condition="pParametrosControl" >
             
                            <p tal:condition="python: pParametrosControl.get( 'from_progress_handler', False)">
                                <strong  tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_ProgressParameters_fromProgressHandler_msgid',  'Progress Parameters from Progress Handler-' )" />
                            </p>
                            <p tal:condition="python: not pParametrosControl.get( 'from_progress_handler', False)">
                                <strong  tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_ProgressParameters_fromPersistentProgress_msgid',  'Progress Parameters from Persistent Progress-' )" />
                            </p>
                            <br/>
                            
                            <p>
                                <span>
                                    <strong  tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_Timestamp_msgid',  'Date and Time-' )" />
                                </span>
                                &nbsp;
                                <span  tal:content="python: pParametrosControl.get( 'timestamp', False)" />
                            </p>
                            <br/>
                            <form method="post" id="cid_TRAConfigureProgress_form" enctype="multipart/form-data">
                                
                                <input name="form_submit" style="font-size: 10pt; font-style: italic"  value="Save" i18n:domain="plone" i18n:attributes="value label_save" type="submit"/>
                                <br/>
                             
                                <table id="cid_TRAProgress_ControlParms" class="listing" tal:define="pClasesFilas python: [ 'even', 'odd',]" >
                                    <thead>
                                        <tr>
                                            <th colspan="3" align="left"  class="nosort">
                                                <font size="2">
                                                    <strong tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_attr_parametrosControl_label', 'Control Parameters-')" />
                                                </font>
                                            </th>
                                        </tr>
                                    </thead>

                                    
                                    
                                    
                                    <tal:block tal:define="pParametroControl_RunAfterPrevious python: pParametrosControl.get( 'RunAfterPrevious', False)" >
                                        <thead>
                                            <tr>
                                                <td tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_runAfterPrevious_label', 'Run After Previous')"/>
                                                <td align="center" >
                                                    <tal:block tal:condition="pParametroControl_RunAfterPrevious"  tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_runAfterPrevious_Yes_label', 'Yes')"/> 
                                                    <tal:block tal:condition="not: pParametroControl_RunAfterPrevious"  tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_runAfterPrevious_No_label', 'No')"/> 
                                                </td>
                                                <td align="center"  tal:define="pName string:theRunAfterPrevious">
                                                    <tal:block tal:condition="pParametroControl_RunAfterPrevious" >
                                                        <span tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_runAfterPrevious_Yes_label', 'Yes')"/> 
                                                        &nbsp;
                                                        <input type="radio" class="noborder" checked="checked" value="Yes" name="#" id="#"
                                                            tal:attributes="name pName; id string:cid_CtrlParms_RunAfterPrevious_Yes" />
                                                        &emsp;&emsp;
                                                        <span tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_runAfterPrevious_No_label', 'No')"/> 
                                                        <input type="radio" class="noborder" value="No" name="#" id="#"
                                                            tal:attributes="name pName; id string:cid_CtrlParms_RunAfterPrevious_No" />
                                                    </tal:block>
                                                    <tal:block tal:condition="not: pParametroControl_RunAfterPrevious" >
                                                        <span tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_runAfterPrevious_Yes_label', 'Yes')"/> 
                                                        &nbsp;
                                                        <input type="radio" class="noborder" value="Yes" name="#" id="#"
                                                            tal:attributes="name pName; id string:cid_CtrlParms_RunAfterPrevious_Yes" />
                                                        &emsp;&emsp;
                                                        <span tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_runAfterPrevious_No_label', 'No')"/> 
                                                        <input type="radio" class="noborder" checked="checked"  value="No" name="#" id="#"
                                                            tal:attributes="name pName; id string:cid_CtrlParms_RunAfterPrevious_No" />
                                                    </tal:block>

                                                </td>
                                            </tr>
                                        </thead>
                                    </tal:block>
                                    
                                    
                                    
                                    <tal:block tal:define="pParametrosControl_StoreResults python: pParametrosControl.get( 'StoreResults', None)" >
                                        <tal:block tal:condition="python: pParametrosControl_StoreResults" >
                                            <thead>
                                                <tr>
                                                    <th colspan="3" align="left"  class="nosort">
                                                        <strong tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_parametrosControl_StoreResults_label', 'Store Results-')" />
                                                    </th>
                                                </tr>
                                            </thead>
                                            <tbody tal:define="global pRowIndex python: 0;
                                                pParametrosControl_StoreResults_Enabled python: pParametrosControl_StoreResults.get( 'enabled', False)" >
                                                <tr class="#" tal:define="global pRowIndex python: pRowIndex + 1"  tal:attributes="class python: pClasesFilas[ pRowIndex % 2]">
                                                    <td tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_parametrosControl_enabled_label', 'Enabled')"/>
                                                    <td align="center" tal:content="python: '%s' % pParametrosControl_StoreResults_Enabled" />
                                                    <td align="center"  tal:define="pName string:theStoreResults_Enabled">
                                                        <tal:block tal:condition="pParametrosControl_StoreResults_Enabled" >
                                                            <span tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_parametrosControl_enabled_label', 'Enabled')"/> 
                                                            &nbsp;
                                                            <input type="radio" class="noborder" checked="checked" value="Enabled" name="#" id="#"
                                                                tal:attributes="name pName; id string:cid_CtrlParms_StoreResults_Enabled_True" />
                                                            &emsp;&emsp;
                                                            <span tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_parametrosControl_disabled_label', 'Disabled')"/> 
                                                            <input type="radio" class="noborder" value="Disabled" name="#" id="#"
                                                                tal:attributes="name pName; id string:cid_CtrlParms_StoreResults_Enabled_False" />
                                                        </tal:block>
                                                        <tal:block tal:condition="not: pParametrosControl_StoreResults_Enabled" >
                                                            <span tal:content="python: here.fTranslateI18N( 'gvSIGi18n', 'gvSIGi18n_TRAProgreso_parametrosControl_enabled_label', 'Enabled')"/> 
                                                            &nbsp;
                                                            <input type="radio" class="noborder" value="Enabled" name="#" id="#"
                                                                tal:attributes="name pName; id string:cid_CtrlParms_StoreResults_Enabled_True" />
